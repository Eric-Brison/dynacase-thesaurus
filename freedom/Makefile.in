PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = @PACKAGE@
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR= Xml Images Css Action JavaScript Class

include $(utildir)/PubRule

DISTFILES = $(SUBDIR) configure.in $(PACKAGE).app \
            $(PACKAGE)_init.php.in Makefile.in $(PACKAGE).spec.in $(PACKAGE).attributes.php\
            VERSION RELEASE \
	    $(PACKAGE)_en.po $(PACKAGE)_fr.po 




