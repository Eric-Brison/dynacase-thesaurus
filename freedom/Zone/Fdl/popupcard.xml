<a href="#" id="[idview]"     
    onclick="activate(this,'[CORE_STANDURL]&app=GENERIC&action=GENERIC_EDIT&vid=[idview]&id=[id]','',true);closeMenu('popupcard');">[txtview]</a>
[ENDBLOCK SEDIT]
</div>
