<!-- ------------ common latest --------------------- -->
<a href="#" id="delete"     
    onclick="if ((this.className == 'menuItem') || (this.className == 'menuItemCtrl'))  {ok =confirm('[TEXT:delconfirm] [dtitle] ([ftitle])'); if (ok) activate(this,'[CORE_STANDURL]&app=GENERIC&action=GENERIC_DEL&id=[id]','');}closeMenu('popupcard');">
[TEXT:Delete]</a>


[BLOCK SUBMENU]
<a  href="#" id="[idmenu]"       
    onmouseover="openSubMenu(event,this,'popup[idmenu]');">[labelmenu] &gt;</a>
[ENDBLOCK SUBMENU]
<a href="#" id="cancel"  onclick="closeMenu('popupcard')"  >[TEXT:Cancel]</a>
[IF SEP]
<a class="separator"></a>
[ENDIF SEP]

[BLOCK ADDLINK]
<a href="#" id="[idlink]"  
    onclick="if (([confirm] && confirm('[tconfirm]')) || (! [confirm])) activate(this,'[url]','[target]',[barmenu]);closeMenu('popupcard');">
[descr]</a>

[ENDBLOCK ADDLINK]

</div>
<div  id="popupview" name="popupcard" class="popup">
[BLOCK SVIEW]
<a href="#" id="[idview]"     
    onclick="activate(this,'[CORE_STANDURL]&app=FDL&action=FDL_CARD&vid=[idview]&id=[id]','',true);closeMenu('popupcard');">[txtview]</a>
[ENDBLOCK SVIEW]
</div>
<div  id="popupedit" name="popupcard" class="popup">
