    onClick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=DEFATTR&id=[id]','info');closeMenu('popupcard')"  >[TEXT: edit attributes]</a>

<a href="#" id="lockdoc" 
    onClick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=LOCKFILE&id=[id]','doc_properties');closeMenu('popupcard')"  >[TEXT: lock]</a>

