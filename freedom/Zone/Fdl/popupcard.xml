<!-- ------------ POPUP DIVISION --------------------- -->
<div  id="popupcard" class="popup">




<a href="#" id="chicon"     
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=QUERYFILE&id=[id]','doc_properties');closeMenu('popupcard');">
[TEXT: change icon]</a>

<a href="#" id="editdoc"     
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=FREEDOM_EDIT&id=[id]','info');closeMenu('popupcard')"  >[TEXT: edit]</a>

<a href="#" id="editattr"     
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=DEFATTR&id=[id]','info');closeMenu('popupcard')"  >[TEXT: edit attributes]</a>

<a href="#" id="lockdoc" 
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=LOCKFILE&id=[id]','doc_properties');closeMenu('popupcard')"  >[TEXT: lock]</a>

<a href="#" id="revise" 
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=REVCOMMENT&id=[id]','doc_properties');closeMenu('popupcard')" >[TEXT: revise]</a>

<a href="#" id="chgtitle" 
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=QUERYTITLE&id=[id]','doc_properties');closeMenu('popupcard')" >[TEXT: change title]</a>

<a href="#" id="unlockdoc"
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=UNLOCKFILE&id=[id]','doc_properties');closeMenu('popupcard')" >[TEXT: unlock]</a>


<a href="#" id="histo"
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=HISTO&id=[id]','info');closeMenu('popupcard')" >[TEXT: history]</a>

<a href="#" id="editprof"
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=EDITPROF&id=[id]','doc_properties');closeMenu('popupcard')" >[TEXT: change profile]</a>

<a href="#" id="editcprof"
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=EDITPROF&create=1&id=[id]','doc_properties');closeMenu('popupcard')" >[TEXT: change creation profile]</a>

<a href="#" id="properties"     
    onclick="activate(this,'[CORE_STANDURL]&app=FREEDOM&action=FREEDOM_CARD&id=[id]','doc_properties');closeMenu('popupcard');">
[TEXT: view properties]</a>

<a href="#" id="access"     
    onclick="activate(this,'[CORE_STANDURL]app=FREEDOM&action=FREEDOM_ACCESS&id=[id]','access');closeMenu('popupcard');">
[TEXT:goaccess]...</a>

<a href="#" id="delete"     
    onclick="ok =confirm('[TEXT:delconfirm] [deltitle]'); if (ok) activate(this,'[CORE_STANDURL]&app=FREEDOM&action=FREEDOM_DEL&id=[id]','doc_properties');closeMenu('popupcard');">
[TEXT: delete]</a>

<a href="#" id="cancel"  
    onclick="closeMenu('popupcard')"  >[TEXT:cancel]</a>
