


<table class="tableborder"  cellspacing="0" width="100%"
     style="cursor:crosshair;border-bottom: 0pt"
     onContextMenu="openMenu(event,'popupcard',1);return false" 
     onMouseDown="if (shiftKeyPushed(event)) openMenu(event,'popupcard',1)"      >
<thead>
<tr class="FREEDOMBack1">
  
 <td  colspan="2">
   <span class="FREEDOMTextBigTitle">[TITLE]
    </span>
  </td>
  <td align="right"  >
<a href="#" onclick="self.print()">
      <img   alt="[TEXT:print]" title="[TEXT:print]" border="0" src="[IMG:print.gif]">
      </a>   
  </td>
</tr>

[BLOCK PROP]
<tr class="FREEDOMBack2"><td  colspan="3"><img src="[IMG:1x1.png]" border="0"></td></tr>
<tr class="FREEDOMBack1">
  <td   align="center" rowspan="[nprop]">
    <a href="[CORE_STANDURL]&app=FREEDOM&action=FREEDOM_CARD&id=[cid]"><IMG id="1"  border="0"  title="[iconalt]" alt="[iconalt]" align="absbottom" width="50px" SRC="[iconsrc]"><br>
     <span class="FREEDOMText" style="font-size:50%">[classtitle]</span></a></td>
  <td   class="FREEDOMText">[TEXT:author]</td>
  <td  ><span class="FREEDOMText">[username]</span></td>
  </td>
</tr>
<tr class="FREEDOMBack1" >  
  <td    class="FREEDOMText"><span style="display:[viewstate]" >[TEXT:state]</span></td>
  <td   ><span style="display:[viewstate]" class="FREEDOMText">[state]</span></td>
  </td>
</tr>
[ENDBLOCK PROP]
[BLOCK LOCK]
<tr class="FREEDOMBack1">
  <td class="FREEDOMText">[TEXT:revision]</td>
  <td  ><span class="FREEDOMText">[revision]</span></td>
</tr>
<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT:locked by]</td>
  <td  ><span class="FREEDOMText">[locked]</span></td>
</tr>
[ENDBLOCK LOCK]
[BLOCK ALLPROP]
<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT:revision date]</td>
  <td  ><span class="FREEDOMText">[revdate]</span></td>
</tr>

<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT: profile access]</td>
  <td  ><span style="display:[displaylprof]" class="FREEDOMText"><a href="[CORE_STANDURL]&app=FREEDOM&action=FREEDOM_CARD&id=[profid]">[profile]</a></span><span style="display:[displayprof]" class="FREEDOMText">[profile]</span></td>
</tr>
<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT:reference]</td>
  <td  ><span class="FREEDOMText">[reference]</span></td>
</tr>
[ENDBLOCK ALLPROP]
</thead>

</table>


