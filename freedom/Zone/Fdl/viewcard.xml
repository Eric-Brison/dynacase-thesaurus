[BLOCK HEAD]
<table class="noprint"  cellspacing="0" width="100%"   >
<thead>
<tr class="FREEDOMBack1">
  
 <td  colspan="2">
<IMG  style="display:[dicon]" border="0"  title="[classtitle]" alt="[classtitle]" align="absbottom" width="50px" SRC="[iconsrc]">
   <span class="FREEDOMTextBigTitle">[TITLE]
    </span>
  </td>
  <td align="right" valign="bottom" >
[BLOCK ACTIONS]
<a href="#" style="display:[amail]" onclick="subwindow(400,400,'fdoc','[CORE_STANDURL]&app=FDL&action=EDITMAIL&mid=[id]')">
      <img  align="absmiddle"  alt="[TEXT:print]" title="[TEXT:senddoc]" height="22" width="30" border="0" src="[IMG:mail.gif]">
      </a>   
<a href="#" onclick="self.print()">
      <img align="absmiddle"   alt="[TEXT:print]" title="[TEXT:print]"  border="0" src="[IMG:print.gif]">
      </a>   <br><B style="display:[dicon]">[state]</B>
[ENDBLOCK ACTIONS]
  </td>
</tr>

[BLOCK PROP]
<tr class="FREEDOMBack2"><td  colspan="3" height="1px"></td></tr>
<tr class="FREEDOMBack1">
  <td   align="center" rowspan="[nprop]">
    <a href="[CORE_STANDURL]&app=FDL&action=FDL_CARD&id=[cid]"><IMG   border="0"  title="[classtitle]" alt="[classtitle]" align="absbottom" width="50px" SRC="[iconsrc]"><br>
     <span class="FREEDOMText" style="font-size:50%">[classtitle]</span></a></td>
  <td   class="FREEDOMText">[TEXT:author]</td>
  <td  ><span class="FREEDOMText">[username]</span></td>
  </td>
</tr>
<tr class="FREEDOMBack1" >  
  <td    class="FREEDOMText"><span style="display:[viewstate]" >[TEXT:state]</span></td>
  <td   ><span style="display:[viewstate]" class="FREEDOMText"><A href="[CORE_STANDURL]&app=FDL&action=FDL_CARD&id=[wid]">[state]</a></span></td>
  </td>
</tr>
<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT:locked by]</td>
  <td  ><span class="FREEDOMText">[locked]</span></td>
</tr>
[ENDBLOCK PROP]
[BLOCK LOCK]
<tr class="FREEDOMBack1">
  <td class="FREEDOMText">[TEXT:revision]</td>
  <td  ><span class="FREEDOMText">[revision]</span></td>
</tr>
[ENDBLOCK LOCK]
[BLOCK ALLPROP]
<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT:revision date]</td>
  <td  ><span class="FREEDOMText">[revdate]</span></td>
</tr>

<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT: profile access]</td>
  <td  ><span style="display:[displaylprof]" class="FREEDOMText"><a href="[CORE_STANDURL]&app=FDL&action=FDL_CARD&id=[profid]">[profile]</a></span><span style="display:[displayprof]" class="FREEDOMText">[profile]</span></td>
</tr>
<tr class="FREEDOMBack1">
  <td   class="FREEDOMText">[TEXT:reference]</td>
  <td  ><span class="FREEDOMText">[reference]</span></td>
</tr>
[ENDBLOCK ALLPROP]
</thead>

</table>

[ENDBLOCK HEAD]

