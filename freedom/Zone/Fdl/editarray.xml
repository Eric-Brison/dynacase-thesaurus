<fieldset ><legend style="cursor:pointer" onclick="viewornot('T[attrid]')">[caption]</legend>
<table width="100%" border=1 cellspacing="0" id="T[attrid]" class="tarray">
   <thead><tr><td width="40px"></td>
    [BLOCK TATTR]<th colspan="2" style="visibility:[ahvis]" width="[ahw]">[alabel]</th> [ENDBLOCK TATTR]
   </tr></thead>
 <tbody id="tbody[attrid]" >
  
     [BLOCK EATTR]<tr onmouseup="sdrag(event);" ><td width="50px" align="left"><img  name="trash" style="visibility:hidden" title="[TEXT:delete selected]" onclick="delseltr()" src="[IMG:trash22.gif]"><img  src="[IMG:next22.gif]"  title="[TEXT:select]" onmousedown="selecttr(this,this.parentNode.parentNode);adrag(event,this);"></td>
    [BLOCK [bevalue]]<td style="width:[vhw];background-color:[bgcolor];">[eivalue]</td>[ENDBLOCK [bevalue]]
 </tr>[ENDBLOCK EATTR]
<tr id="lasttr[attrid]">
<td class="und" width="50px" align="right" >
  <img  name="unselect" style="visibility:hidden" title="[TEXT:unselect]" align="bottom" onclick="unseltr()" src="[IMG:point22.gif]"><img name="unselect" style="visibility:hidden" src="[IMG:dup22.gif]" title="[TEXT:duplicate]" onclick="duptr();return false;">   
</td><td colspan="[footspan]"><img id="tiadd[attrid]" src="[IMG:add22.gif]" title="[TEXT:add]" onclick="addtr('tnew[attrid]','tbody[attrid]');return false;"></td></tr>
  </tbody>
   <tfoot><tr style="display:none;" onmouseup="sdrag(event);" id="tnew[attrid]"><td width="50px" align="left"><img   name="trash" style="visibility:hidden" title="[TEXT:delete selected]" onclick="delseltr()" src="[IMG:trash22.gif]"><img  src="[IMG:next22.gif]"  title="[TEXT:select]" onmousedown="selecttr(this,this.parentNode.parentNode);adrag(event,this);"></td>
    [BLOCK IATTR]<td  style="background-color:[bgcolor];width:[ihw]">[ilabel]</td> [ENDBLOCK IATTR]
   </tr>
  </tfoot>
</table>

</fieldset>
