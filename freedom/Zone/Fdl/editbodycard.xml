<!-- TABLEBODY -->
[BLOCK TABLEBODY]


<input type="hidden" name="title" value="[TITLE]"></input>

<fieldset id="F[frameid]" [IF TAB]name="[tag]" style="display:none" [ENDIF TAB]class="mfield" ><legend style="cursor:pointer" onclick="viewornot('T[frameid]')">[frametext]</legend>

<div id="T[frameid]" >
            [BLOCK [TABLEVALUE]]
<table style="width:100%" cellspacing="0" class="edittable">
<tr class="[classback]">
            [IF SINGLEROW]
  <td class="[labelclass]"  width="30%"><span >[name]&nbsp;:&nbsp;</span></td>
  <td >[inputtype]</td>
            [ENDIF SINGLEROW]
            [IFNOT SINGLEROW]
  
  <td colspan="3">[IF viewlabel]<span class="[labelclass]" >[name]&nbsp;:&nbsp;</span><br>[ENDIF viewlabel][inputtype]</td>
            [ENDIF SINGLEROW]
</tr>
</table>
           [ENDBLOCK [TABLEVALUE]]
</div>
</fieldset>
[ENDBLOCK TABLEBODY]



<div style="display:none">

[BLOCK HIDDENS]
[inputtype]
[ENDBLOCK HIDDENS]
</div>

[IF ONETAB]
<div id="ttabs" class="tabsh" >
[BLOCK TABS]
<span  name="spantabs" onmousedown="showFieldset(event,this,'TAG[tabid]')">[tabtitle]</span>
[ENDBLOCK TABS]</div>
<div class="line" id="toptab"></div>
  <script>moveFieldset();showFirstFieldset()</script>
[ENDIF ONETAB]
