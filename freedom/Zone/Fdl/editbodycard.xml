<!-- TABLEBODY -->
[BLOCK TABLEBODY]


<input type="hidden" name="title" value="[TITLE]"></input>

<fieldset ><legend style="cursor:pointer" onclick="viewornot('T[frameid]')">[frametext]</legend>

<div id="T[frameid]" >
            [BLOCK [TABLEVALUE]]
<table style="width:100%">
<tr class="[classback]">
            [BLOCK [NORMALROW]]
  <td class="[labelclass]"  width="30%"><span >[name]&nbsp;:&nbsp;</span></td>
  <td >[inputtype]</td>
            [ENDBLOCK [NORMALROW]]
            [BLOCK [ARRAYROW]]
  
  <td colspan="3">[inputtype]</td>
            [ENDBLOCK [ARRAYROW]]
</tr>
</table>
           [ENDBLOCK [TABLEVALUE]]
</div>
</fieldset>
[ENDBLOCK TABLEBODY]



<div style="display:none">

[BLOCK HIDDENS]
[inputtype]
[ENDBLOCK HIDDENS]
</div>
