[BLOCK TABLEBODY]

<fieldset id="F[frameid]" [IF TAB]name="[tag]" style="display:none" [ENDIF TAB]class="mfield" ><legend style="cursor:pointer" onclick="viewornot('T[frameid]')">[frametext]</legend>
<table width="100%" id="T[frameid]" >

<tr class="FREEDOMBack1">

  <td colspan="2"  height="1px"></td>
  <td  width="0%" rowspan="[rowspan]" align="center" >[BLOCK [IMAGES]]<A href="[imgsrc]" target="[itarget]"><IMG border="0" title="[imgalt]" alt="[imgalt]" align="absbottom" width="80px" SRC="[imgthumbsrc]"></A><br>[ENDBLOCK [IMAGES]]</td>

</tr>
[BLOCK [TABLEVALUE]]

<tr  class="[classback]">
  <td valign="top"  class="FREEDOMLabel"  width="[wvalue]"><span style="display:[ndisplay]">[name]&nbsp;:&nbsp;</span></td>
  <td valign="top" class="FREEDOMText" >[value]</td>
</tr>

[ENDBLOCK [TABLEVALUE]]
</table>
</fieldset>
[ENDBLOCK TABLEBODY]

[IF ONETAB]
<div id="ttabs" class="tabsh" >
[BLOCK TABS]
<span  name="spantabs" onclick="showFieldset(event,this,'TAG[tabid]')">[tabtitle]</span>
[ENDBLOCK TABS]</div>
<div class="line" id="toptab"></div>
  <script>moveFieldset();showFirstFieldset()</script>
[ENDIF ONETAB]
