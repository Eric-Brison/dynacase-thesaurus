<?xml version="1.0" encoding="UTF-8"?>
<status count="[count]" delay="[delay]" code="[CODE]" warning="[warning]" >
  <branch><![CDATA[



<div   id="popupdoc" class="popup" >




[BLOCK ADDLINK]
[IF URL]
<a href="#" id="[idlink]"  visibility="[visibility]" submenu="[smid]"
    onmouseup="if (([confirm] && confirm('[tconfirm]')) || (! [confirm])) sendMenuUrl(this,'[url]','[target]',[barmenu],'[mwidth]','[mheight]');closeDocMenu();">
[IF ICONS]<img src="[icon]">[ENDIF ICONS][descr]</a>[ENDIF URL]
[IF JSFT]
<a href="#" id="[idlink]"  visibility="[visibility]" submenu="[smid]"
    onmouseup="if (([confirm] && confirm('[tconfirm]')) || (! [confirm])) {[jsfunction];}closeDocMenu();">
[IF ICONS]<img src="[icon]">[ENDIF ICONS][descr]</a>[ENDIF JSFT]
[IF separator]    
<a class="separator"></a>[ENDIF separator]
[IF issubmenu]
<a  href="#" id="[idlink]"       visibility="2"
    onmouseover="openSubDocMenu(event,this,'popup[idlink]');">[descr] &gt;</a>
[ENDIF issubmenu]
[ENDBLOCK ADDLINK]


[IF SEP]
<a class="separator"></a>
[ENDIF SEP]

</div>
<div  id="popupview" name="popupcard" class="popup">
[BLOCK SVIEW]
<a href="#" id="[idview]"     
    onclick="sendMenuUrl(this,'[CORE_STANDURL]&app=FDL&action=FDL_CARD&vid=[idview]&id=[id]','',true);closeDocMenu();">[txtview]</a>
[ENDBLOCK SVIEW]
</div>
<div  id="popupedit" name="popupcard" class="popup">
[BLOCK SEDIT]
<a href="#" id="[idview]"     
    onclick="sendMenuUrl(this,'[CORE_STANDURL]&app=GENERIC&action=GENERIC_EDIT&vid=[idview]&id=[id]','',true);closeDocMenu();">[txtview]</a>
[ENDBLOCK SEDIT]
</div>

[BLOCK SUBDIVMENU]
<div  id="popup[idlink]" name="popupcard" class="popup"></div>   
[ENDBLOCK SUBDIVMENU]


]]></branch>
</status>
