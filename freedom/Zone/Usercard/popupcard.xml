<!-- ------------ POPUP DIVISION --------------------- -->
<div  id="popupcard" class="popup">




<a href="#" id="editdoc"     
    onclick="activate(this,'[CORE_STANDURL]&app=USERCARD&action=USERCARD_EDIT&id=[id]','info');closeMenu('popupcard')"  >[TEXT: edit]</a>





<a href="#" id="unlockdoc"
    onclick="activate(this,'[CORE_STANDURL]&app=USERCARD&action=UNLOCKFILE&id=[id]','info');closeMenu('popupcard')" >[TEXT: unlock]</a>


<a id="vcard" href="[CORE_STANDURL]&app=USERCARD&action=USERCARD_VCARD&id=[id]" >[TEXT: vcard]</a>
<a href="#" id="chgcatg"
    onclick="activate(this,'[CORE_STANDURL]&app=USERCARD&action=USERCARD_EDITCHANGECATG&id=[id]','info');closeMenu('popupcard')" >[TEXT: change category]</a>

<a href="#" id="properties"     
    onclick="activate(this,'[CORE_STANDURL]&app=USERCARD&action=USERCARD_CARD&id=[id]','info');closeMenu('popupcard');">[TEXT: view properties]</a>

<a href="#" id="delete"     
    onclick="ok =confirm('[TEXT:delconfirm] [deltitle]'); if (ok) activate(this,'[CORE_STANDURL]&app=USERCARD&action=USERCARD_DEL&id=[id]','info');closeMenu('popupcard'); if (ok) parent.list.location.reload();">[TEXT: delete]</a>

<a href="#" id="cancel"  
    onclick="closeMenu('popupcard')"  >[TEXT:cancel]</a>
</div>
<!-- ------------------ end POPUP DIVISION ------------------ -->



