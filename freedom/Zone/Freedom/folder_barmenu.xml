<!-- ------------------ BAR MENU ------------------ -->
<div id="barmenu" style="position:fixed;left:0;top:0;cursor:default;z-index:6;" >
<table  class="barmenu"  cols="4"  cellspacing="0"  >
<thead>
<tr>

<td class="CORETblHead" onClick="closeAllMenu();" colspan="2" ><div style="max-height:30px;overflow:hidden"><B>[title]</B> [nbdoc]</div></td>
<td  class="MenuInactive"  
     onClick="closeAllMenu();selectMenu(this);openMenuXY(event,'viewmenu',this.offsetLeft, this.offsetTop+this.offsetHeight+getScrollYOffset());return false" 
     onMouseOver="ActiveMenu(this)"
     onMouseOut="DeactiveMenu(this)">
[TEXT:View]
</td>
<td  class="MenuInactive"  
     onClick="closeAllMenu();selectMenu(this);openMenuXY(event,'toolmenu',this.offsetLeft, this.offsetTop+this.offsetHeight+getScrollYOffset());return false" 
     onMouseOver="ActiveMenu(this)"
     onMouseOut="DeactiveMenu(this)">
[TEXT:Tools]
</td>
</tr>
</thead>

</table >
</div>

<br>
