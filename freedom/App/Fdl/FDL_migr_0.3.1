#!/bin/bash

if [ "$dbpsql" == "" ]; then
    #load environement variable for freedom
  . /etc/freedom.conf
   wchoose -b
fi



psql $dbfree -f $wpub/FDL/fdl.sql
$wpub/wsh.php --api=fdl_trigger --trigger=yes | psql $dbfree

echo "update doc set lmodify='L' where id in (select distinct on (initid) id from doc where locked = -1  order by initid, revision desc);" | psql $dbfree
