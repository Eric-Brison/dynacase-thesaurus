#!/bin/bash
if [ "$dbpsql" == "" ]; then
    #load environement variable for freedom
  . /etc/freedom.conf
   wchoose -b
fi

echo "alter table doc add column prelid int;" | psql $dbfree
echo "alter table doc add column fldrels int[];" | psql $dbfree
psql $dbfree -f $wpub/FDL/fdl.sql
/usr/bin/php -q $wpub/API/updateclass.php --dbcoord="$dbfree" --appc=FDL --class=QueryDir
$wpub/wsh.php --api=migr_2.3.1 | psql $dbfree