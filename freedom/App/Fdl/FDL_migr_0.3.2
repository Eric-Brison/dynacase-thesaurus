#!/bin/bash

if [ "$dbpsql" == "" ]; then
    #load environement variable for freedom
  . /etc/freedom.conf
   wchoose -b
fi


echo "create table docfrom ( id int not null, primary key (id), fromid int);" | psql $dbfree
echo "insert INTO docfrom (id, fromid) select id, fromid from doc;" | psql $dbfree
echo "update docfrom set fromid=-1 where id in (select id from docfam);" | psql $dbfree

$wpub/wsh.php --api=fdl_pkey  | psql $dbfree


psql $dbfree -f $wpub/FDL/fdl.sql
$wpub/wsh.php --api=fdl_trigger --trigger=Y | psql $dbfree
