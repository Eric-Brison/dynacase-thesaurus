echo "update  docattr set id=str_replace(id,' ','') where id like '% ' ;" | psql $dbfree
echo "DELETE from docattr where id='rep_style';" | psql $dbfree
$wpub/FDL/FDL_post U
echo "UPDATE doc5 set se_sqlselect = fld.query, ba_title=title from fld where doc5.initid = fld.dirid and ba_title is null and doctype != 'Z' and doctype != 'T' and query is not null;" | psql $dbfree
