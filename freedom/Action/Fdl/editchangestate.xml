[ZONE FDL:HTMLHEAD?title=[thetitle]]

<form  class="fborder" 
       id="fedit" 
       name="modifydoc" 
       onsubmit="this.target=window.parent.name;if (document.isSubmitted) return false;if (!canmodify()) return false;document.isSubmitted=true;enableall();viewwait(true);return true"
       method="POST"  
       action="[CORE_STANDURL]&app=FREEDOM&action=MODSTATE&id=[docid]" >
<input type="hidden" name="newstate" value="[nextstate]">
<table ><tbody>
[BLOCK FINPUTS]
<tr style="display:[idisplay];"  id="TWF[aid]" class="FREEDOMBack1">
  <td  valign="top" class="[labelclass]">[alabel]&nbsp;:&nbsp;</td>
  <td>[avalue]</td>
</tr>
[ENDBLOCK FINPUTS]</tbody>
<tfoot>
<tr >
  <td  colspan="3" align="right" id="editbutton"><input id="iSubmit" style="background-color:[wcolor]" type="submit" title="[tonewstate]" value="[tostate]"   >
    <input type="button" title="[TEXT:Cancel the change state]" value="[TEXT:Cancel]" onclick="window.parent.hidebox('POPDOC')"> 
  </td>
</tr>
</tfoot>
</table>

</form>
