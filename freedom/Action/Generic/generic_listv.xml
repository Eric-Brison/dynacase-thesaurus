function vedit(e,id) {
  if (!e) e=window.event;

  if (e.ctrlKey) {
    subwindow([FDL_VD2SIZE],[FDL_HD2SIZE],'fedit'+id,'[CORE_STANDURL]&app=GENERIC&action=GENERIC_EDIT&latest=Y&id='+id);  
