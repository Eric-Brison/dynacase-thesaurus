<html>
  <head>

   <title>[TEXT: folder containt]</title>
<link rel="shortcut icon" href="FDL/Images/logo16.ico" >
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS" >
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=FDL&action=FDL_CSS" >

[CSS:REF]


<style type="text/css">
[CSS:CODE]


TH {
  font-family: [CORE_FONTLIST1];
  font-size: [CORE_FONTBASESIZE];
  font-weight: normal;  
  font-style: italic;
  text-align:left;
  border-bottom: 1px solid;
}


</style>

[JS:REF]

   <script language="JavaScript1.2">
  <!--
    [JS:CODE]
    document.dirid='[dirid]';

-->
</script>   
 </head>


<body class="freedom" topmargin=1 leftmargin=1
      onLoad="initmicon();" 
      onClick="closeMenu('popuplist');" >



<img id="micon" width="20" height="20"   style="position:absolute;visibility:hidden;z-index:10" border="0" src="FREEDOM/Images/doc.gif">


[ZONE FREEDOM:FOLDER_BARMENU?title=[dirtitle]&nbdoc=[nbdoc]&dirid=[dirid]]
[ZONE FREEDOM:POPUP]



                    [BLOCK TABLEBODY]
<table class="tableborder" cellspacing="0" style="margin-top:5pt" width="100%" >
<thead>
<tr><th><IMG class="iconimg20"   align="absmiddle"  src="[iconfamsrc]"><B>[ftitle]</B></th>
[BLOCK [blockattr]]
<th>[aname]</th>
[ENDBLOCK [blockattr]]</tr></thead>
<tbody id="tbody[fid]">
[BLOCK [blockvalue]]
<tr  id="d[divid]"
     class="unselect" 
     onMouseUp="if (! document.noselect) {select(this,[id],[divid]);openMenuOrProperties(event,'popuplist',[divid]);};document.noselect=false;"      
     onContextMenu="select(this,[id],[divid]);openMenu(event,'popuplist',[divid]);return false" 
     onDblClick="if ([isfld]) openFld([id]); else sendFirstFile([id]);return false"
     onmouseover="oselect([id])"  onmouseout="ounselect()"
     title="[highlight]">   
 <td class="tlist">
<IMG style="display:none" id="i[divid]"  src="[iconsrc]">[title] <i>[state]</i></td>       
<td class="tlist">[values]</td>        
</tr>
[ENDBLOCK [blockvalue]]
</tbody>
<script>alterrow('tbody[fid]','[CORE_BGCOLORALTERN]');</script>
</table>
                    [ENDBLOCK TABLEBODY]



<!--  FOOT HTML -->

</body>
</html>
