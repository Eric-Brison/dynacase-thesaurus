<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS" >
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SSTANDURL]app=FDL&action=FDL_CSS" >
[IF RSS]<link rel="alternate" type="application/rss+xml" title="[foldername]" href="[CORE_STANDURL]&app=FREEDOM&action=FREEDOM_RSS&id=[dirid]"/>[ENDIF RSS]

[CSS:REF]


<style type="text/css">
[CSS:CODE]


TH {
  font-weight: normal;  
  font-style: italic;
  text-align:left;
  border-bottom: 1px solid;
}


</style>

[JS:REF]

   <script language="JavaScript1.2">
  <!--
    [JS:CODE]
    document.dirid='[dirid]';
function alterbody(id) {
  [IF VALTERN]
alterrow('tbody'+id,'[CORE_BGCOLORALTERN]','[CORE_TEXTFGCOLOR]');
[ENDIF VALTERN]
