


<!-- ------------------ BAR MENU ------------------ -->
<div id="barmenu" style="position:[divpos];left:0;top:0;cursor:default;">
<table  class="tablesimple"  cols="4"  cellspacing="0"   >
<thead>
<tr>
<td  class="MenuInactive"  
