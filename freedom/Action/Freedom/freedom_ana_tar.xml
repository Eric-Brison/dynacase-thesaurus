
<!-- HEAD HTML -->

<html>
  <head>

   <title>[TEXT:Import Result] </title>


<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS" >
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=FDL&action=FDL_CSS" >

[CSS:REF]


   <style type="text/css">
[CSS:CODE]
.dir {
  font-weight: bold;
  list-style: square;
  color:red;
}
.file {
  font-weight: medium;
  color:black;
  list-style: circle;
}
   </style>

[JS:REF]

   <script language="JavaScript">
  <!--
    [JS:CODE]
  //-->
   </script>   



 </head>

<body class="freedom">

[BLOCK AFORM]
<form  class="fborder" 
       name="anatar" 
       method="POST" 
       target="i_bgtaranalyze"
       ENCTYPE="multipart/form-data" 
       action="[CORE_STANDURL]&app=[APPNAME]&action=FREEDOM_ANA_TAR" >

      

   <H1>[TEXT:Analyze Archive]</H1>

<input type="hidden" name="dirid" value="[dirid]"></input> 
<input type="hidden" name="analyze" value="Y"> </input> 
<input type="hidden" name="filename" value="[selfile]"></input> 
<table width="100%" cols="2">
<tr><td>
<fieldset >
<legend>[TEXT:Options import]</legend> 
   [TEXT:for]
   <select name="famid" >  
         [BLOCK SELECTCLASS]
           <option VALUE="[idcdoc]" [selected]>[classname]</option>   
         [ENDBLOCK SELECTCLASS]
    </select><BR>

<input type="checkbox" value="1" [oselected] name="onlycsv">[TEXT:Only CSV]</input><br/>
<input type="checkbox" name="topfld" value="1" [tselected]>[TEXT:Add top folder]</input> 
<hr width="50%">
<input type="submit" value="[TEXT:Analyze]"> 
</fieldset></td><td>
<fieldset width="40%">
<legend>[TEXT:Lauch Import]</legend> 
<label for="to">[TEXT:report to (mail address)]</label>
<input type="text" size=30 name="to" value=[mailaddr]></input><br><hr width="50%">
<input id="bgimport" type="submit" [bgdisabled]
       onclick="document.anatar.analyze.value='N';document.anatar.action='[CORE_STANDURL]&app=[APPNAME]&action=FREEDOM_IMPORT_DIR'" 
       value="[importtext]">
</fieldset>
</td></tr></table>
</form>

[ENDBLOCK AFORM]

<fieldset>
<legend>[TEXT:Analysis Results]</legend> 
<div style="overflow:auto;height:240px;">
<table class="[huge]" >

<thead><tr><th>[TEXT:foldername]</th><th>[TEXT:filename]</th><th>[TEXT:familyname]</th><th>[TEXT:action]</th><th>[TEXT:err]</th></tr></thead>
<tbody  >
[BLOCK ADDEDDOC]
<tr><td>[foldername]</td><td>[filename]</td><td>[familyname]</td><td>[action]</td><td>[err]</td></tr>
[ENDBLOCK ADDEDDOC]
</tbody></table>
</div>
</fieldset>




</body>
</html>
