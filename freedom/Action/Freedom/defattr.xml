
<!-- HEAD HTML -->

<html>
  <head>

   <title>FREEDOM - [TEXT:Attributes definition]</title>


<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS" >
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SSTANDURL]app=FDL&action=FDL_CSS" >

[CSS:REF]


   <style type="text/css">
[CSS:CODE]
   </style>

[JS:REF]

   <script language="JavaScript">
  <!--
    [JS:CODE]
  //-->
   </script>   



 </head>

<body class="freedom" onload="autoWresize()">

                         


<!-- Title Table -->
<form target="fdoc" class="fborder" name="modifyfamilly" method="POST"  action="[CORE_STANDURL]&app=[APPNAME]&action=MODATTR&docid=[docid]&dirid=[dirid]" >


                         

<input type="hidden" name="title" value="[TITLE]"> 


<table class="tableborder"  width="100%" cellspacing="0" cellpadding="0" >
<tr>
 <td class="FREEDOMTblTitle">
   <span class="FREEDOMTextTitle">[TITLE]</span>
  </td> 
  <td class="FREEDOMTblTitle" align="right"><span class="FREEDOMTextName">[TEXT:inherit from]</span>
   <select name="classid" onChange="document.location.href='[CORE_STANDURL]&app=[APPNAME]&action=DEFATTR&id='+self.document.modifyfamilly.classid.options[self.document.modifyfamilly.classid.selectedIndex].value">   
           <option VALUE="0">[TEXT:no document family]</option>      
         [BLOCK SELECTCLASS]
           <option VALUE="[idcdoc]" [selected]>[classname]</option>   
         [ENDBLOCK SELECTCLASS]
    </select>
   </td>
</tr>
</table>

<!-- Frame Table -->


<table  class="tablesimple"   cellspacing="0" cellpadding="0" >
<thead>
<tr>
  <td class="CORETblHeadBold">[TEXT:ID]</td>
  <td class="CORETblHeadBold">[TEXT:order]</td>
  <td class="CORETblHeadBold">[TEXT:name]</td>
  <td class="CORETblHeadBold">[TEXT:type]</td>
  <td class="CORETblHeadBold">[TEXT:frame]</td>
  <td class="CORETblHeadBold">[TEXT:A?]</td>
  <td class="CORETblHeadBold">[TEXT:T?]</td>
  <td class="CORETblHeadBold">[TEXT:N?]</td>
  <td class="CORETblHeadBold">[TEXT:Vi]</td>
  <td class="CORETblHeadBold">[TEXT:Link]</td>
  <td class="CORETblHeadBold">[TEXT:file]</td>
  <td class="CORETblHeadBold">[TEXT:func]</td>
  <td class="CORETblHeadBold">[TEXT:elink]</td>
  <td class="CORETblHeadBold">[TEXT:constraint]</td>
  <td class="CORETblHeadBold">[TEXT:options]</td>
</tr>
</thead>
<tbody>


[BLOCK NEWELEM]
<tr>
   <td >
   <input type="hidden" name="attrid[[neweltid]]" value="[attrid]" >
      <input type="text" name="nattrid[[neweltid]]" value="[attrid]"  [disabledid]>
   </td>
   <td  >

      <input type="text" name="order[[neweltid]]" value="[order]" size="3" [disabled]>
   </td>
   <td  >
      <input type="text" name="name[[neweltid]]" value="[attrname]" [disabled]>
   </td>
   <td  >
      <input type="text" name="type[[neweltid]]" value='[typevalue]' [disabled]>      
   </td>
   <td  >
      <select name="frameid[[neweltid]]"  [disabled]>        
         [BLOCK [SELECTFRAME]]
           <option VALUE="[frameid]" [selected]>[framevalue]</option>
         [ENDBLOCK [SELECTFRAME]]
    </select>
   </td>
   <td  >
      <input type="checkbox" value="Y" name="abstractyn[[neweltid]]" [abscheck] [disabled]></input>
   </td>
   <td  >
      <input type="checkbox" value="Y" name="titleyn[[neweltid]]" [titcheck] [disabled]></input>
   </td>
   <td  >
      <input type="checkbox" value="Y" name="neededyn[[neweltid]]" [neededcheck] [disabled]></input>
   </td>
   <td  >
      <input type="text" size="1"  name="visibility[[neweltid]]" value="[visibility]" [disabled]></input>
   </td>
   <td  >
      <input type="text"   name="link[[neweltid]]" value="[link]" [disabled]></input>
   </td>
   <td  >
      <input type="text"  name="phpfile[[neweltid]]" value="[phpfile]" [disabled]></input>
   </td>
   <td  >
      <input type="text"   name="phpfunc[[neweltid]]" value="[phpfunc]" [disabled]></input>
   </td>
   <td  >
      <input type="text"   name="elink[[neweltid]]" value="[elink]" [disabled]></input>
   </td>
   <td  >
      <input type="text"   name="phpconstraint[[neweltid]]" value="[phpconstraint]" [disabled]></input>
   </td>
   <td  >
      <input type="text"   name="options[[neweltid]]" value="[options]" [disabled]></input>
   </td>
  
</tr>
[ENDBLOCK NEWELEM]
<tr>
  <td colspan="11" >
 <input type="submit" value="[TEXT:Validate]"
        onclick="setTimeout('self.close()',2000)"> 
  </td>
</tr>

</tbody>

</table>


</form>
<!--  FOOT HTML -->

</body>
</html>
