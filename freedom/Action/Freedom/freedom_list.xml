<html>
  <head>

   <title>[TEXT: folder containt]</title>


<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS" >
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=FDL&action=FDL_CSS" >

[CSS:REF]


<style type="text/css">
[CSS:CODE]
.iconimg {
  border-style: hidden;
  border-width: 1;
  border-color: [CORE_FGCOLOR];
  width:20;
  height:20;
  
}
.iconline {
  border-color: [CORE_FGCOLOR];
  margin:1px;
  padding:0px;
  border-width:3px;
  border-style: hidden;
  /*background-color: palegoldenrod;*/
  
}

TD.tlist {
   border-width : 1px;
   border-style : dashed none none none;
   border-color : [CORE_BGCOLOR];
   font-size: [CORE_FONTBASESIZE];
}

TR.unselect {
  cursor: crosshair;

}


TR.select {
  background-color:[CORE_BGCOLORALTERN];
  border-style : solid;
  cursor: crosshair;
} 


</style>

[JS:REF]

   <script language="JavaScript1.2">
  <!--
    [JS:CODE]
    document.dirid='[dirid]';

-->
</script>   
 </head>

<body 
      onLoad="initmicon();" topmargin=1 leftmargin=1
      onClick="closeMenu('popuplist');" 
      onResize="">





<img id="micon" width="20" height="20"   style="position:absolute;visibility:hidden;z-index:10" border="0" src="FREEDOM/Images/doc.gif">


[ZONE FREEDOM:FOLDER_BARMENU?nbdoc=[nbdoc]&dirid=[dirid]]
[ZONE FREEDOM:POPUP]
<br>
<table class="tablesimple" cellspacing="0" width="100%" >
<!-- TABLEBODY -->
                    [BLOCK TABLEBODY]
<tr  id="d[divid]"
     class="unselect"
     onMouseDown="select(this,[id],[divid]);openMenuOrProperties(event,'popuplist',[divid]);" 
     onContextMenu="openMenu(event,'popuplist',[divid]);return false" 
     onDblClick="if ([isfld]) openFld([id]); else sendFirstFile([id]);return false"
     >
<td class="tlist" width="30" >
<IMG class="iconimg" id="i[divid]" 
     align="absmiddle"  src="[iconsrc]">
</td>
<td class="tlist" >  
  
     [title]

</td>
     <td class="tlist"  width="40" ><I style="font-size:80%">[revdate]</I></td>
     <td class="tlist"  width="20">[locked]&nbsp;</td>
     <td class="tlist"  width="20" ><sup style="font-size:80%">[revision]&nbsp;</sup></td>

    
</tr>

                    [ENDBLOCK TABLEBODY]
</table>
<!-- /TABLEBODY -->





