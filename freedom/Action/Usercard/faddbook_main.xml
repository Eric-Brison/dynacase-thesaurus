<html>

<head>
<link rel="shortcut icon" href="FDL/Images/logo16.ico">
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS&layout=USERCARD:faddbook.css">


[JS:REF]

<script type="text/javascript">

[JS:CODE]

function sendform(evt) {
  evt = (evt) ? evt : ((event) ? event : null );
  var cc = (evt.keyCode) ? evt.keyCode : evt.charCode;
  if (cc == 13) {
    document.getElementById('suser').submit();
    return false;
  }
  return true;
}

function manageCols() {
  subwindow(600, 200, 'colums', '[CORE_STANDURL]&app=[APPNAME]&action=FADDBOOK_MAINCOLS&dfam=[dfam]');
}

</script>

</head>

<body class="fabbody">

<form style="display:none" id="setline" name="setline" method="POST" target="_self" action="[CORE_STANDURL]&app=[APPNAME]&action=FADDBOOK_SETUPARAM">
<input type="hidden" name="pname" id="pname" value="FADDBOOK_MAINLINE">
<input type="hidden" name="pvalue" id="pvalue" value="[linep]">
<input type="hidden" name="rapp" value="[APPNAME]">
<input type="hidden" name="raction" value="FADDBOOK_MAIN&dfam=[dfam]">
</form>


<form style="display:none" id="addpref" name="addpref" method="POST" target="faddbook_prefered" action="[CORE_STANDURL]&app=[APPNAME]&action=FADDBOOK_ADDPREFERED">
<input type="hidden" name="cid" id="cid" value="-1">
</form>

<form style="display:none" id="suserv" name="suserv" method="POST" target="_self" action="[CORE_STANDURL]&app=[APPNAME]&action=FADDBOOK_MAIN&dfam=[dfam]"></form>
<form id="suser" name="suser" method="POST" target="_self" action="[CORE_STANDURL]&app=[APPNAME]&action=FADDBOOK_MAIN&dfam=[dfam]">

<table width="100%" align="center" cellspacing="0">
<tr class="fabzone">
<td colspan="[colspan]">

<table width="100%" align="center" cellspacing="0">
<td><img title="Page précédente" style="visibility:[IF PrevPage]visible[ENDIF PrevPage][IFNOT PrevPage]hidden[ENDIF PrevPage]; cursor:pointer; border:0" src="[IMG:faddbook_prevpage.gif]" onclick="getElementById('sp').value='[pprev]'; getElementById('suser').submit();">
<img title="Initialiser la recherche..." style="cursor:pointer; border:0" src="[IMG:faddbook_initsearch.gif]" onclick="getElementById('suserv').submit();">
<img title="Page suivante" style="visibility:[IF NextPage]visible[ENDIF NextPage][IFNOT NextPage]hidden[ENDIF NextPage]; cursor:pointer; border:0" src="[IMG:faddbook_nextpage.gif]" onclick="getElementById('sp').value='[pnext]'; getElementById('suser').submit();">
</td>
<td><span style="font-weight:bold; border:1px outset; cursor:pointer" title="[TEXT:click here to create a new]" onmouseover="this.className='fabcontactsel'" onmouseout="this.className=''"   onclick="subwindow(300, 400, 'newdoc', '[CORE_STANDURL]&app=GENERIC&action=GENERIC_EDIT&classid=[famid]');">[famsearch]&nbsp;<img width="16px" src="[IMG:faddbook_ico_newdoc.png]" border="0"></span></td>
<td class="fabzone">[TEXT:lignes par page] : <select name="linepp" onchange="getElementById('pvalue').value=this.options[this.selectedIndex].value; getElementById('setline').submit();">[BLOCK BLine]<option value="[count]" [init]>[count][ENDBLOCK BLine]</select></td>
</tr>
</table>

</td>
</tr>

<tr class="fabheadlist">

<td width="20px">[IF isManager]<img title="[TEXT:manage columns]" style="cursor:pointer; border:0" src="[IMG:faddbook_cols.gif]" height="16px" onclick="manageCols()">[ENDIF isManager]</td>
[BLOCK COLS]<td nowrap>
[IF ATTimage][label][ENDIF ATTimage]
[IF ATTnormal]<input name="rqi_[id]" type="text" style="[IF filter]color:red;[ENDIF filter]" value="[label]" title="[TEXT:filter on] [label]..." onmouseover="this.style.background='yellow'" onmouseout="this.style.background='';this.style.color='[IF filter]red;[ENDIF filter]'" onclick="this.value=''" onkeypress="return sendform(event);"><img style="border:0; position:relative; left:-20px; top:2px" src="[IMG:faddbook_search.gif]">[ENDIF ATTnormal]</td>[ENDBLOCK COLS]</tr>

[BLOCK DLines]<tr title="[title]"  class="fabcontact"  onmouseover="this.className='fabcontactsel'" onmouseout="this.className='fabcontact'" ><td nowrap align="center"><img title="[TEXT:add to prefered]" height="16" style="cursor:pointer; border:0" src="[icop]" onclick="document.getElementById('cid').value=[cid]; document.getElementById('addpref').submit();"></td>[BLOCK C[Line]]<td nowrap onclick="subwindow(300, 400, '[TEXT:detail]', '[CORE_STANDURL]&app=FDL&action=FDL_CARD&props=N&abstract=N&id=[cid]&zone=[fabzone]'); ">[IF ATTimage]<img border="0" height="16" src="[content]">[ENDIF ATTimage][IF ATTnormal][content][ENDIF ATTnormal]&nbsp;</td>[ENDBLOCK C[Line]]</tr>
[ENDBLOCK DLines]



</table>

<input type="hidden" name="order" id="order" value="[order]">
<input type="hidden" name="sp" id="sp" value="[sp]">
<input type="hidden" name="lp" id="sp" value="[lp]">
<input type="hidden" name="dfam" id="dfam" value="[dfam]">

</form>

<iframe name="faddbook_main_hidden" id="faddbook_main_hidden" style="display:none"></iframe>

</body>
</html>
      
