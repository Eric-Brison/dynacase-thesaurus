<html>
<head>

<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS&layout=USERCARD:faddbook.css">
<link rel="shortcut icon" href="FDL/Images/logo16.ico">
[JS:REF]

<script type="text/javascript">

[JS:CODE]

var lOnglet = [ 'contact', 'address', 'other' ];

function showInfo(id) {
  var elt;
  for (var i=0; i<lOnglet.length; i++) {
    if (elt = document.getElementById(lOnglet[i])) {
      elt.style.display = 'none';
      document.getElementById('o'+lOnglet[i]).className = 'fabog';
      if (lOnglet[i]==id) {
	elt.style.display = '';
	document.getElementById('o'+lOnglet[i]).className = 'fabogsel';
      }	
    }
  }
  return true;
}

</script>

</head>
<body class="fabbody">

<div class="fabcard">

<table width="100%" cellspacing="0" border="0">

<tr>
<td width="80px" align="center">
<img src="[IMG:societe.gif]" width="40px">
</td>
<td>
<div class="fabctitlep">[IF S_SI_SOCIETY]<span class="fabctitle">[V_SI_SOCIETY]</span>&nbsp;[ENDIF S_SI_SOCIETY]</div>
[IF S_SI_TOWN]<div class="fabctitlep"><span class="fabcsoc">[V_SI_TOWN]</span></div>[ENDIF S_SI_TOWN]
</td>
</tr>

<tr><td></td><td><div height="30px">&nbsp;</div></td></tr>
<tr valign="top">

<td class="fabogcontent" align="center">
<div id="ocontact" class="fabogsel" onmouseout="if (this.className!='fabogsel') this.className='fabog'" onmouseover="if (this.className!='fabogsel') this.className='fabogover'" onclick="showInfo('contact')">[TEXT:contact]</div>
<div id="oaddress" class="fabog" onmouseout="if (this.className!='fabogsel') this.className='fabog'" onmouseover="if (this.className!='fabogsel') this.className='fabogover'" onclick="showInfo('address')">[TEXT:address]</div>
<div id="oother" class="fabog" onmouseout="if (this.className!='fabogsel') this.className='fabog'" onmouseover="if (this.className!='fabogsel') this.className='fabogover'" onclick="showInfo('other')">[TEXT:other]</div>
</td>

<td>

<div class="fabogdetail" id="contact"><table width="100%">

[IF S_SI_MAIL]<tr>
<td align="right"><span class="fabccom">[L_SI_MAIL]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_MAIL]</span></td>
</tr>[ENDIF S_SI_MAIL]

<tr><td colspan="3">&nbsp;</td><tr>

[IF S_SI_PHONE]<tr>
<td align="right"><span class="fabccom">[L_SI_PHONE]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_PHONE]</span></td>
</tr>[ENDIF S_SI_PHONE]
[IF S_SI_MOBILE]<tr>
<td align="right"><span class="fabccom">[L_SI_MOBILE]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_MOBILE]</span></td>
</tr>[ENDIF S_SI_MOBILE]
[IF S_SI_FAX]<tr>
<td align="right"><span class="fabccom">[L_SI_FAX]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_FAX]</span></td>
</tr>[ENDIF S_SI_FAX]

<tr><td colspan="3">&nbsp;</td><tr>

[IF S_SI_WEB]<tr>
<td align="right"><span class="fabccom">[L_SI_WEB]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_WEB]</span></td>
</tr>[ENDIF S_SI_WEB]

<tr><td colspan="3">&nbsp;</td><tr>

</table></div>

<div style="display:none" class="fabogdetail" id="address"><table width="100%">

[IF S_SI_ADDR]<tr><td></td>
<td colspan="2"><span class="fabcrub">[V_SI_ADDR]</span></td></tr>[ENDIF S_SI_ADDR]
<tr><td></td>
<td colspan="2"><span class="fabcrub">[IF S_SI_POSTCODE][V_SI_POSTCODE] [ENDIF S_SI_POSTCODE][V_SI_TOWN][IF S_SI_CEDEX]</span><span class="fabccom"> [TEXT:cedex] </span> <span class="fabcrub">[V_SI_CEDEX]</span>[ENDIF S_SI_CEDEX]</td></tr>
[IF S_SI_COUNTRY]<tr><td></td><td colspan="2"><span class="fabcrub">[V_SI_COUNTRY]</span></td></tr>[ENDIF S_SI_COUNTRY]


<tr><td></td><td colspan="2" align="left"><div class="fabcaddress">[TEXT:other sites]</div><div>&nbsp;</div></td></tr>
<tr><td></td>
<tr><td></td><td colspan="2"><span class="fabcrub">[V_SI_SITES]</span></td></tr>

</table>
</div>

<div style="display:none"  class="fabogdetail" id="other"><table width="100%">

[IF S_SI_CATG]<tr>
<td align="right"><span class="fabccom">[L_SI_CATG]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_CATG]</span></td>
</tr>[ENDIF S_SI_CATG]
[IF S_SI_JOB]<tr>
<td align="right"><span class="fabccom">[L_SI_JOB]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_JOB]</span></td>
</tr>[ENDIF S_SI_JOB]
[IF S_SI_DESCR]<tr valign="top">
<td align="right"><span class="fabccom">[L_SI_DESCR]</span></td>
<td align="center"><span class="fabccom">:</span></td>
<td align="left"><span class="fabcrub">[V_SI_DESCR]</span></td>
</tr>[ENDIF S_SI_DESCR]

<tr><td colspan="3">&nbsp;</td><tr>

</table></div>

</td></tr>
</table>


</div>
</body>
</html>
