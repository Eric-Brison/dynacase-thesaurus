  width:30;
  height:30;
  
}
.iconline {
  border-color: [CORE_FGCOLOR];
  margin:1px;
  padding:0px;
  border-width:3px;
  border-style: hidden;
  /*background-color: palegoldenrod;*/
  
}

TD.tlist {
   border-width : 1px;
   border-style : dashed none none none;
   border-color : [CORE_BGCOLOR];
   font-size: 80%;
}

DIV.unselect {
  cursor: crosshair;
   border-style : inset;
  background-color:[CORE_BGCELLCOLOR];
  color:[CORE_TEXTFGCOLOR];
   border-color : [CORE_BGCOLOR];
   border-width : 1px;
   font-size: 80%;
  margin: 2px 0px 0px 0px;

}

DIV.unselecth {
  cursor: crosshair;
   border-style : inset;
  background-color:[CORE_BGCOLORHIGH];
