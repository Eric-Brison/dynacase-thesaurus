   border-width : 1px;
   font-size: 80%;
  margin: 2px 0px 0px 0px;

}

DIV.select {
  background-color:[CORE_BGCOLORALTERN];
  color:[CORE_TEXTFGCOLOR];
  border-style : outset;
  cursor: crosshair;
   border-width : 1px;
   font-size: 80%;
  margin: 2px 0px 0px 0px;
} 

HR {
  color:[CORE_BGCOLOR];
}

.onglet {
