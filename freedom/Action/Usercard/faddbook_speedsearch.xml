<html>

<head>

[ZONE FDL:HTMLHEAD]

<script type="text/javascript">
var isfirst=[first];
var sChange=false;
function sendform(evt) {
  evt = (evt) ? evt : ((event) ? event : null );
  var cc = (evt.keyCode) ? evt.keyCode : evt.charCode;
  if (cc == 13) {
    document.getElementById('suser').submit();
    sChange=false;
    return false;
  }
  isfirst = false;
  sChange=true;
  return true;
}
</script>

</head>
<body class="freedom">

<form id="suser" name="suser" method="POST" target="_self" action="[CORE_STANDURL]&app=USERCARD&action=FADDBOOK_SPEEDSEARCH">
<table width="100%" cellspacing="0">
<tr><td colspan="2" class="tocitem0on">
&nbsp;<img align="absmiddle" height="16" style="border:0" src="[icon]">&nbsp;
<input type="hidden" id="first" value="[first]">
<input size="16" onmouseover="this.style.background='yellow'" onmouseout="this.style.background='';" onclick="if (isfirst) this.value=''" type="text" name="vtext" id="vtext" value="[vtext]">
<img height="16" style="position:relative; left:-20px; top:2px;" src="[IMG:faddbook_search.gif]">
</td></tr>

[IF Result]
[BLOCK Contacts]<tr><td onclick="subwindow(300, 400, '[TEXT:view detail]', '[CORE_STANDURL]&app=FDL&action=FDL_CARD&props=N&abstract=N&id=[id]');"><img height="16" style="border:0" src="[icon]">&nbsp;[title]</td>
</tr>
[ENDBLOCK Contacts][ENDIF Result]
</table>

<iframe name="faddbook_pref_hidden" id="faddbook_pref_hidden" style="display:none"></iframe>
</body>
</html>
