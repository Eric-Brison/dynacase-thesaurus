<script type="text/javascript">

[JS:CODE]

var isfirst=[first];
var sChange=false;
function sendform(evt) {
  evt = (evt) ? evt : ((event) ? event : null );
  var cc = (evt.keyCode) ? evt.keyCode : evt.charCode;
  if (cc == 13) {
    document.getElementById('suser').submit();
    sChange=false;
    return false;
  }
  isfirst = false;
  sChange=true;
  return true;
}
</script>

