<html>
<head>
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=USERCARD&action=FADDBOOK_CSS">
[JS:REF]
<script type="text/javascript">
[JS:CODE]
var isfirst=[first];
var sChange=false;
function sendform(evt) {
  evt = (evt) ? evt : ((event) ? event : null );
  var cc = (evt.keyCode) ? evt.keyCode : evt.charCode;
  if (cc == 13) {
    document.getElementById('suser').submit();
    sChange=false;
    return false;
  }
  isfirst = false;
  sChange=true;
  return true;
}
</script>
</head>
<body class="fabbody">
<form id="suser" name="suser" method="POST" target="_self" action="[CORE_STANDURL]&app=USERCARD&action=FADDBOOK_SPEEDSEARCH">
<input type="hidden" id="first" value="[first]"><input type="hidden" name="dfam" value="[dfam]">
<div class="fabzone" width="100%"><img align="absmiddle" height="16" style="border:0" src="[icon]">&nbsp;[IF bCount]([Count])[ENDIF bCount]&nbsp;<input size="15" onmouseover="this.style.background='yellow'" onmouseout="this.style.background='';" onclick="if (isfirst) this.value=''" type="text" name="vtext" id="vtext" value="[vtext]"><img height="16" style="position:relative; left:-20px; top:2px;" src="[IMG:faddbook_search.gif]"><input style="border:0px" type="checkbox" name="sallf" title="[TEXT:search in all contact fields]"></div>

[IF Result][BLOCK Contacts]
<span border="1" onclick="subwindow(300, 400, '[TEXT:view_detail][id]', '[CORE_STANDURL]&app=FDL&action=FDL_CARD&props=N&abstract=N&id=[id]&zone=[fabzone]');">[resume]</span>[ENDBLOCK Contacts][ENDIF Result]

<iframe name="faddbook_pref_hidden" id="faddbook_pref_hidden" style="display:none"></iframe>
</body>
</html>
