<html>
<head>
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=USERCARD&action=FADDBOOK_CSS">
[JS:REF]

<script type="text/javascript">
[JS:CODE]


var timeValue = 500;
var currentElt = '';
var timerId = -1;

function displayResumeCard() {
  var idr = 'res'+currentElt;
  var idl = 'l'+currentElt;
  var celt = document.getElementById(idr);
  if (!celt) return false;
  document.getElementById(idr).style.display = '';
  document.getElementById(idl).style.display = 'none';
  return true;
}

function hideResumeCard(elt) {
  var idr = 'res'+elt;
  var idl = 'l'+elt;
  var celt = document.getElementById(idr);
  if (!celt) return false;
  document.getElementById(idr).style.display = 'none';
  document.getElementById(idl).style.display = '';
  return true;
}

function initTimer(elt) {
  if (currentElt == elt) return true;
  resetTimer();
  currentElt = elt;
  timerId = self.setTimeout("displayResumeCard()", timeValue);
  return true;
}

function resetTimer() {
  if (timerId!=-1) clearTimeout(timerId);
  timerId = -1;
  currentElt = '';
}
  
</script>

</head>
<body class="fabbody">

<form style="display:none" id="delpref" name="delpref" method="POST" target="_self" action="[CORE_STANDURL]&app=USERCARD&action=FADDBOOK_DELPREFERED">
<input type="hidden" name="cid" id="cid" value="0">
</form>

<table  width="100%" cellspacing="0"><tr>
<td><div class="fabzone"><img onclick="location.reload()" title="[TEXT:click to reload favorites]" style="cursor:pointer" align="absmiddle" height="16" style="border:0" src="[IMG:faddbook_ico_favorite.png]">&nbsp;[TEXT:my favorites]</div></td>
<td width="20px"><div class="fabzone"><img style="cursor:pointer" title="[TEXT:delete all favorites]" onclick="if (confirm('[TEXT:confirm delete all favorite]')) { getElementById('cid').value=-2;getElementById('delpref').submit(); };"  align="absmiddle" height="16" style="border:0" src="[IMG:faddbook_ico_deletefavorite.png]"></td>
</tr></table>



[BLOCK Contacts]
<div class="fabcontact" id="l[id]">
<table width="100%" cellspacing="0">
<td width="12px" align="center" onclick="if (confirm('[TEXT:confirm delete] [jstitle]')) { document.getElementById('cid').value=[id]; document.getElementById('delpref').submit(); }" title="[TEXT:delete from prefered]"><img height="6" style="cursor:pointer; border:0" src="[IMG:faddbook_delpref.gif]"></td>
<td width="20px" align="center"><img height="16" style="border:0" src="[icon]"></td>
<td align="left" onmouseover="document.getElementById('l[id]').className='fabcontactsel'; initTimer([id]);" onmouseout="document.getElementById('l[id]').className='fabcontact'; resetTimer();" onclick="subwindow(300, 400, '[TEXT:view detail]', '[CORE_STANDURL]&app=FDL&action=FDL_CARD&props=N&abstract=N&id=[id]&zone=[fabzone]'); document.getElementById('l[id]').className='fabcontact'" >[title]</td>
</tr></table></div>
<div colspan="2" id="res[id]" style="margin:0px 5px 0px 5px; display:none" onclick="hideResumeCard([id]); document.getElementById('l[id]').className='fabcontact'" width="95%">[resume]</div>
[ENDBLOCK Contacts]

<iframe name="faddbook_pref_hidden" id="faddbook_pref_hidden" style="display:none"></iframe>
</body>
</html>
