# get the new appname (basename in uppercase)
appname = $(shell basename `pwd` | tr [a-z] [A-Z])

export appname


include $(utildir)/PubRule



$(pubdir)/$(appname)/logo16.ico : logo16.ico
	cp $< $(pubdir)/$(appname)/Images
publish: $(pubdir)/$(appname)/logo16.ico 
