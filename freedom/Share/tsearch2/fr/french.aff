	U ë			>	-Uë,U	# = -ë (aiguë)
	U ë			>	-ë,S

	# Finale -(c)que
	[^C] Q U E		>	-QUE,C	# caduque
	[^C] Q U E		>	-QUE,CS	# caduque
	C Q U E			>	-CQUE,C	# = -QUE  (grecque)
	C Q U E			>	-QUE,S

	# Finale -gue
	G U E			>	-GUE,G	# = -UE (longue)
	G U E			>	-UE,S	# = -UE (longue)

	# Finale -che
	[^è] C H E		>	-CHE,C	# = -HE (blanche)
