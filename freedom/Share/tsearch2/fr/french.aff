	# Consonne c + e
	C E			>	-CE,X	# douce

	# Consonne d + e
	D E			>	-DE,D	# = -E (lourde)
	D E			>	-E,S

	# Consonnes ff + e
	F F E			>	-FFE,F	# = -FE (cheffe)
	F F E			>	-FE,S

	# Consonne(s) (l)l + e
