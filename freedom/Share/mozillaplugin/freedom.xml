<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Freedom</ShortName>
<Description>Recherche Freedom</Description>
<InputEncoding>ISO-8859-1</InputEncoding>
<Image width="16" height="16">data:image/x-icon;base64,</Image>
<Url type="text/html" method="GET" template="http://localhost/freedom/">
  <Param name="keyword" value="{searchTerms}"/>
  <Param name="latest" value="true"/>
  <Param name="app" value="FREEDOM"/>
  <Param name="action" value="FULLSEARCH"/>
  <Param name="sole" value="Y"/>
  <Param name="fromdir" value="false"/>
  <Param name="sensitive" value="false"/>
  <Param name="viewone" value=""/>
</Url>
<SearchForm>http://localhost/freedom/freedomsearch.php</SearchForm>
</SearchPlugin>
