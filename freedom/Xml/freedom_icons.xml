[ZONE FREEDOM:POPUP]

<!-- TABLEBODY -->
[BLOCK TABLEBODY]
<div class="icon" id="d[divid]" >
<IMG class="iconimg" id="i[divid]" 
     onMouseDown="select(this,[id],[divid]);openMenuOrAbstract(event);" 
     onContextMenu="openMenu(event,'popup');return false" 
     align="absbottom"  src="[iconsrc]"><br>

  
  <span class="FREEDOMTextTitle">[title]<sup style="font-size:50%">[revision]</sup></span>
    


</div>
<!-- -------------- ABSTRACT DIVISION --------------------- -->
<div id="a[divid]" class="abstract">
<span class="FREEDOMTextAbstractValue">[title]</span>

<ul>
  [BLOCK [blockabstract]]
  <li><span  class="FREEDOMTextAbstract">[name]&nbsp;:&nbsp;[value]</span></li>
  [ENDBLOCK [blockabstract]]
  </ul>
</div>


[ENDBLOCK TABLEBODY]
