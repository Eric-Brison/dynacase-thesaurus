<!-- HEAD HTML -->

<html>
  <head>

   <title>[TEXT:Properties:] [TITLE]</title>

   <style>
[ZONE CORE:GENCSS]
   </style>


[CSS:REF]


   <style type="text/css">
[CSS:CODE]
   </style>

[JS:REF]

   <script language="JavaScript">
  <!--
    [JS:CODE]
  //-->
  var docid=[id];
  var selid=1;
   </script>
 </head>

<body class="freedom" 
      onClick="closeMenu('popupcard');" >




<!-- ------------ POPUP DIVISION --------------------- -->
<div  id="popupcard" class="popup">


<a href="#" id="chicon"     
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=QUERYFILE&id='+docid,'doc_properties');closeMenu('popupcard');">
[TEXT: change icon]</a>

<a href="#" id="editdoc"     
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_EDIT&id='+docid,'doc_properties');closeMenu('popupcard')"  >[TEXT: edit]</a>

<a href="#" id="editattr"     
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=DEFATTR&id='+docid,'doc_properties');closeMenu('popupcard')"  >[TEXT: edit attributes]</a>

<a href="#" id="lockdoc" 
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=LOCKFILE&id='+docid,'doc_properties');closeMenu('popupcard')"  >[TEXT: lock]</a>

<a href="#" id="revise" 
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=REVISION&id='+docid,'doc_properties');closeMenu('popupcard')" >[TEXT: revise]</a>

<a href="#" id="unlockdoc"
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=UNLOCKFILE&id='+docid,'doc_properties');closeMenu('popupcard')" >[TEXT: unlock]</a>


<a href="#" id="ctrldoc"
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=CTRLDOC&id='+docid,'doc_properties');closeMenu('popupcard')" >[TEXT: set control]</a>

<a href="#" id="editprof"
    onClick="activate(this,'[CORE_STANDURL]&app=[FREEDOM_APP]&action=EDITPROF&id='+docid,'doc_properties');closeMenu('popupcard')" >[TEXT: change profile]</a>


<a href="#" id="cancel"  
    onClick="closeMenu('popupcard')"  >cancel</a>
</div>
<!-- ------------------ end POPUP DIVISION ------------------ -->


<!-- Title Table -->

<table class="tableborder" cellspacing="0" width="100%" >
<tr>
  
 <td class="FREEDOMTblTitle">
   <span class="FREEDOMTextBigTitle">[TITLE]
    </span>
  </td>
  <td align="right"  class="FREEDOMTblTitle">
<a href="#" onClick="subwindow(300,500,'FREEDOM_ACCESS','[CORE_STANDURL]app=[FREEDOM_APP]&action=FREEDOM_ACCESS&id=[id]')">[imgaccess]</a>
<a   type="text/x-vcard" href="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_ONEEXPORT&conv_type=Vcard&id=[id]"><img  alt="[TEXT:oneexport]" title="[TEXT:oneexport]" border="0" src="[IMG:export.gif]"></a>
<a href="#" onclick="self.print()">
      <img   alt="[TEXT:print]" title="[TEXT:print]" border="0" src="[IMG:print.gif]">
      </a>
<a   href="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_DEL&id=[id]" onClick="ok=confirm('[TEXT:delconfirm] [deltitle]'); if (ok) setTimeout('parent.list.document.location.reload()',1); return (ok);" >[imgdel]</a>
   
  </td>
</tr>
</table>



<p></p>

<table class="tableborder"  cellspacing="0" width="100%"
     onContextMenu="openMenu(event,'popupcard');return false" 
     onMouseDown="if (shiftKeyPushed(event)) openMenu(event,'popupcard')"      >
<tr>

  <td class="FREEDOMTblFrame" colspan="3" ><span class="FREEDOMTextTitle">[TEXT:document attributes]</span></td>
</tr>

<tr>
  <td class = "tdstyle" width="10%" align="center" rowspan="4">
    <a href="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_CARD&id=[cid]"><IMG id="1"  border="0" title="[iconalt]" alt="[iconalt]" align="absbottom" width="50" SRC="[iconsrc]"><br>
     <span class="FREEDOMTextValue" style="font-size:50%">[classtitle]</span></a>
  </td>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT:reference]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[reference]</span></td>
</tr>
<tr>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT:revision]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[revision]</span></td>
</tr>

<tr>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT:locked by]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[locked]</span></td>
</tr>
<tr>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT: profile access]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[profile]</span></td>
</tr>

<!-- TABLEBODY -->
[BLOCK TABLEBODY]


<tr>

  <td class="FREEDOMTblFrame" colspan="3" ><span class="FREEDOMTextTitle"> [frametext]</span></td>
</tr>

<tr>

  <td class = "tdstyle" width="10%" rowspan="[rowspan]" align="center"><img src="[IMG:1x1.png]" border="0">[BLOCK [IMAGES]]<A href="[imgsrc]" ><IMG border="0" title="[imgalt]" alt="[imgalt]" align="absbottom" width="50" SRC="[imgsrc]"></A><br>[ENDBLOCK [IMAGES]]</td>
  <td class = "tdstyle"  colspan="2"><img src="[IMG:1x1.png]" border="0"></td>

</tr>
[BLOCK [TABLEVALUE]]

<tr>

  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[name]&nbsp;:&nbsp;</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[value]</span></td>
</tr>

[ENDBLOCK [TABLEVALUE]]

[ENDBLOCK TABLEBODY]

</table>
<P class="FREEDOMTextCopyright">[TEXT:createdby] [username]</P>


</body>
</html>
