
<!-- HEAD HTML -->

<html>
  <head>

   <title>[TEXT:Properties:] [TITLE]</title>

   <style>
[ZONE CORE:GENCSS]
   </style>


[CSS:REF]


   <style type="text/css">
[CSS:CODE]
   </style>

[JS:REF]

   <script language="JavaScript">
  <!--
    [JS:CODE]
  //-->
  var docid=[id];
  var selid=1;
   </script>
 </head>

<body class="freedom"  >




<!-- Title Table -->
<div class="TITLE">
<table width="100%" border="0" cellspacing="0" cellpadding="0"frame="box" rules="none" >
<tr>
  
 <td class="FREEDOMTblTitle">
   <span class="FREEDOMTextBigTitle">[TITLE]
    </span>
  </td>
  <td align="right"  class="FREEDOMTblTitle">
<a href="#" onClick="subwindow(300,500,'FREEDOM_ACCESS','[CORE_STANDURL]app=[FREEDOM_APP]&action=FREEDOM_ACCESS&id=[id]')">[imgaccess]</a>
<a   type="text/x-vcard" href="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_ONEEXPORT&conv_type=Vcard&id=[id]"><img  alt="[TEXT:oneexport]" title="[TEXT:oneexport]" border="0" src="[IMG:export.gif]"></a>
<a href="#" onclick="self.print()">
      <img   alt="[TEXT:print]" title="[TEXT:print]" border="0" src="[IMG:print.gif]">
      </a>
<a   href="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_DEL&id=[id]" onClick="ok=confirm('[TEXT:delconfirm] [deltitle]'); if (ok) setTimeout('parent.list.document.location.reload()',1); return (ok);" >[imgdel]</a>
   <a    href="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_EDIT&id=[id]">[imgedit]</a>
   <a    href="[CORE_STANDURL]&app=[FREEDOM_APP]&action=DEFATTR&id=[id]">[imgedit]</a>
  </td>
</tr>
</table>
</div>




[ZONE FREEDOM:POPUP]


<table 
     onContextMenu="openMenu(event,'popup');return false" 
     onMouseDown="if (shiftKeyPushed(event)) openMenu(event,'popup')"
     border="0" width="100%" frame="box" rules="all" cellspacing="0" cellpadding="0" >
<tr>

  <td class="FREEDOMTblFrame" colspan="3" ><span class="FREEDOMTextTitle">[TEXT:document attributes]</span></td>
</tr>

<tr>
  <td class = "tdstyle" width="10%" align="center" rowspan="4">
    <IMG id="1"  border="0" title="[iconalt]" alt="[iconalt]" align="absbottom" width="50" SRC="[iconsrc]"><br>
     <span class="FREEDOMTextValue" style="font-size:50%">[classtitle]</span>
  </td>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT:reference]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[reference]</span></td>
</tr>
<tr>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT:state]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[state]</span></td>
</tr>
<tr>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT:revision]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[revision]</span></td>
</tr>

<tr>
  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[TEXT:locked by]</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[locked]</span></td>
</tr>

<!-- TABLEBODY -->
[BLOCK TABLEBODY]


<tr>

  <td class="FREEDOMTblFrame" colspan="3" ><span class="FREEDOMTextTitle"> [frametext]</span></td>
</tr>

<tr>

  <td class = "tdstyle" width="10%" rowspan="[rowspan]" align="center"><img src="[IMG:1x1.png]" border="0">[BLOCK [IMAGES]]<A href="[imgsrc]" ><IMG border="0" title="[imgalt]" alt="[imgalt]" align="absbottom" width="50" SRC="[imgsrc]"></A><br>[ENDBLOCK [IMAGES]]</td>
  <td class = "tdstyle"  colspan="2"><img src="[IMG:1x1.png]" border="0"></td>

</tr>
[BLOCK [TABLEVALUE]]

<tr>

  <td class = "tdstyle" width="30%"><span class="FREEDOMTextName">[name]&nbsp;:&nbsp;</span></td>
  <td class = "tdstyle" width="60%"><span class="FREEDOMTextValue">[value]</span></td>
</tr>

[ENDBLOCK [TABLEVALUE]]

[ENDBLOCK TABLEBODY]

</table>
<P class="FREEDOMTextCopyright">[TEXT:createdby] [username]</P>


</body>
</html>
