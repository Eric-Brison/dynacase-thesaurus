 </head>

<body class="freedom">

                         


<!-- Title Table -->
<form  target="doc_properties" class="fborder" name="modifydoc" method="POST" ENCTYPE="multipart/form-data" action="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_MOD&id=[id]&dirid=[dirid]" >

      


<table class="tableborder"  cellspacing="0" width="100%">
<tr>
 <td class="FREEDOMTblTitle" colspan="2">
   <span class="FREEDOMTextBigTitle">[TITLE]</span>
  </td> 
   <td class="FREEDOMTblTitle" align="right">[TEXT:inherit from]
   <select name="classid" onChange="document.location.href='[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_EDIT&id=[id]&dirid=[dirid]&classid='+self.document.modifydoc.classid.options[self.document.modifydoc.classid.selectedIndex].value">  
         [BLOCK SELECTCLASS]
           <option VALUE="[idcdoc]" [selected]>[classname]</option>   
         [ENDBLOCK SELECTCLASS]
    </select>
   </td>
</tr>


<!-- TABLEBODY -->
[BLOCK TABLEBODY]


<tr>

  <td class="FREEDOMTblFrame" colspan="3" ><span class="FREEDOMTextTitle"> [frametext]</span></td>
</tr>



[BLOCK [TABLEVALUE]]

<tr>

  <td class = "tdstyle" width="10%">&nbsp;</td>
  <td class = "tdstyle"  width="30%"><span class="FREEDOMTextName">[name]&nbsp;:&nbsp;</span></td>
  <td class = "tdstyle" width="60%">[inputtype]</td>
</tr>

[ENDBLOCK [TABLEVALUE]]

[ENDBLOCK TABLEBODY]

<tr>
  <tr>
  <td colspan="3" class = "tdstyle">
 <input type="submit" value=[editaction] onClick="document.location.href='[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_LOGO'"> 
  </td>
  </tr>

</table>


<INPUT TYPE="hidden" name="MAX_FILE_SIZE" value="[UPLOAD_MAX_FILE_SIZE]"></input>
<input type="hidden" name="title" value="[TITLE]"></input>
</form>
<!--  FOOT HTML -->

</body>
</html>
