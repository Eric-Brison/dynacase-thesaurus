 <input type="submit" value=[editaction] onClick="document.location.href='[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_LOGO'"> 
  </td>
</tr>

</table>


<INPUT TYPE="hidden" name="MAX_FILE_SIZE" value="[UPLOAD_MAX_FILE_SIZE]"></input>
<input type="hidden" name="title" value="[TITLE]"></input>
