<tr class="FREEDOMBack2" >

  <td  colspan="3" ><span class="FREEDOMTextTitle"> [frametext]</span></td>
</tr>



[BLOCK [TABLEVALUE]]

<tr class="FREEDOMBack1">

  <td  width="10%">&nbsp;</td>
  <td  width="30%"><span class="FREEDOMText">[name]&nbsp;:&nbsp;</span></td>
  <td  width="60%">[inputtype]</td>
</tr>

[ENDBLOCK [TABLEVALUE]]

[ENDBLOCK TABLEBODY]


<tr class="FREEDOMBack1">
