   <script language="JavaScript">
  <!--
    [JS:CODE]

function sendmodifydoc(docid, attrid) {


  f =document.modifydoc;
  // modify to initial action
  oldact = f.action;
  oldtar = f.target;
  f.action = '[CORE_STANDURL]&app=[FREEDOM_APP]&action=ENUM_CHOICE&docid='+docid+'&attrid='+attrid;
  w= window.open('about:blank',
              'choose',
              'resizable=yes,scrollbars=yes,width=100,height=100,top=100,left=100');
  w.focus();
  f.target='choose';

  f.submit();
  // reset to initial action
  f.action=oldact;
  f.target=oldtar;

}
  //-->
   </script>   



 </head>

<body class="freedom">

<form method="POST" target="_blank" name="choose"></form>


<!-- Title Table -->
<form  target="doc_properties" class="fborder" name="modifydoc" method="POST" ENCTYPE="multipart/form-data" action="[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_MOD&id=[id]&dirid=[dirid]" >

      


<table class="tableborder"  cellspacing="0" width="100%">
<tr class="FREEDOMBack1">
 <td  colspan="2">
   <span class="FREEDOMTextBigTitle">[TITLE]</span>
  </td> 
   <td  class="FREEDOMText" align="right">[TEXT:inherit from]
   <select name="classid" onChange="document.location.href='[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_EDIT&id=[id]&dirid=[dirid]&classid='+self.document.modifydoc.classid.options[self.document.modifydoc.classid.selectedIndex].value">  
         [BLOCK SELECTCLASS]
           <option VALUE="[idcdoc]" [selected]>[classname]</option>   
         [ENDBLOCK SELECTCLASS]
    </select>
   </td>
</tr>

<tr class="FREEDOMBack2"><td  colspan="3"><img src="[IMG:1x1.png]" border="0"></td></tr>

<!-- TABLEBODY -->
