
<!-- HEAD HTML -->

<html>
  <head>

   <title>[WHAT] [APP_TITLE]</title>

   <style>
[ZONE CORE:GENCSS]
   </style>


[CSS:REF]


   <style type="text/css">
[CSS:CODE]
   </style>

[JS:REF]

   <script language="JavaScript">
  <!--
    [JS:CODE]
  //-->
   </script>   



 </head>

<body class="freedom">

                         


<!-- Title Table -->
<form target="doc_properties" class="fborder" name="modifyfreedom" method="POST" ENCTYPE="multipart/form-data" action="[CORE_STANDURL]&app=[FREEDOM_APP]&action=MODATTR&docid=[docid]" >

      <INPUT TYPE="hidden" name="MAX_FILE_SIZE" value="[UPLOAD_MAX_FILE_SIZE]">
                         

<input type="hidden" name="title" value="[TITLE]"> 

<div class="TITLE">
<table width="100%"  cellspacing="0" cellpadding="0" >
<tr>
 <td class="FREEDOMTblTitle">
   <span class="FREEDOMTextTitle">[TITLE]</span>
  </td> 
  <td class="FREEDOMTblTitle" align="right"><span class="FREEDOMTextName">[TEXT:inherit from]</span>
   <select name="classid" >   
           <option VALUE="0">[TEXT:no document family]</option>      
         [BLOCK SELECTCLASS]
           <option VALUE="[idcdoc]" [selected]>[classname]</option>   
         [ENDBLOCK SELECTCLASS]
    </select>
   </td>
</tr>
</table>
</div>
<!-- Frame Table -->


<table border="0" width="100%" border="0" cellspacing="0" cellpadding="0" >
<thead>
<tr>
  <td><span class="FREEDOMTextName">order</span></td>
  <td><span class="FREEDOMTextName">name</span></td>
  <td><span class="FREEDOMTextName">type</span></td>
  <td><span class="FREEDOMTextName">frame</span></td>
  <td><span class="FREEDOMTextName">A?</span></td>
  <td><span class="FREEDOMTextName">T?</span></td>
</tr>
</thead>
<tbody>


[BLOCK NEWELEM]
<tr>
   <td class = "tdstyle" >
   <input type="hidden" name="attrid[[neweltid]]" value="[attrid]" >
      <input type="text" name="order[[neweltid]]" value="[order]" size="3" [disabled]>
   </td>
   <td class = "tdstyle" >
      <input type="text" name="name[[neweltid]]" value="[attrname]" [disabled]>
   </td>
   <td class = "tdstyle" >
      <select name="type[[neweltid]]"  [disabled]>        
         [BLOCK [SELECTOPTION]]
           <option VALUE="[typevalue]" [selected]>[typevalue]</option>   
         [ENDBLOCK [SELECTOPTION]]
    </select>
   </td>
   <td class = "tdstyle" >
      <select name="frameid[[neweltid]]"  [disabled]>        
         [BLOCK [SELECTFRAME]]
           <option VALUE="[frameid]" [selected]>[framevalue]</option>
         [ENDBLOCK [SELECTFRAME]]
    </select>
   </td>
   <td class = "tdstyle" >
      <input type="checkbox" value="Y" name="abstractyn[[neweltid]]" [abscheck] [disabled]></input>
   </td>
   <td class = "tdstyle" >
      <input type="checkbox" value="Y" name="titleyn[[neweltid]]" [titcheck] [disabled]></input>
   </td>
  
</tr>
[ENDBLOCK NEWELEM]
<tr>
  <td colspan="3" class = "tdstyle">
 <input type="submit" value="[TEXT:modify]"
        onClick="document.location.href='[CORE_STANDURL]&app=[FREEDOM_APP]&action=FREEDOM_LOGO'"> 
  </td>
</tr>

</tbody>

</table>


</form>
<!--  FOOT HTML -->

</body>
</html>
