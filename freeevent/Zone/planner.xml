<script>
mstart=[mstart];
mend=[mend];
document.docid=[id];
document.vid='[vid]';
document.zone='[zone]';
document.needreload=true;
document.moreurl='[ppar]';
addEvent(window,"load",placeEvents);
</script>


<div id="binter" style="position:absolute;display:none">
<input onclick="allcal()" title="[TEXT:Allperiod]"  type="image" src="[IMG:forback.gif]"></input>
<input onclick="isFixed=true;placeEvents();style.visibility='hidden'" type="image" src="[IMG:pins.gif]" title="[TEXT:Fixbar]" ></input><br>
<input id="nperiod" value="[dday100]" title="[TEXT:Period in day]" type="text" size="2" ></input>
<input onclick="resizecal()" title="[TEXT:Change day period]" type="image" src="[IMG:forward.gif]"></input>
</div>
<div id="dday"  name="dday" style="display:none" onclick="viewcal(this.id)"  >XX</div>

<div id="dhour" name="dhour" title="HH" style="position:absolute;left=0px;top=0px;display:none;"><table class="rowright24" cellspacing=0 width="100%" cols="24"><tr><td>1</td><td>2</td><td>3</td><td>4</td><td>5</td><td>6</td><td>7</td><td>8</td><td>9</td><td>10</td><td>11</td><td>12</td><td>13</td><td>14</td><td>15</td><td>16</td><td>17</td><td>18</td><td>19</td><td>20</td><td>21</td><td>22</td><td>23</td><td>24</td></tr></table></div>




<table id="tcalhead" width="100%" border="1" style="background-color:transparent">
<thead>
<tr><td  align="left">[begdate]<button onclick="movecal(-100)" >&lt;[dday100]</button><button onclick="movecal(-50)" >&lt;[dday50]</button><button onclick="movecal(-10)">&lt;[dday10]</button></td>
<td><button onclick="zoomx=zoomx*2;placeEvents();" title="[TEXT:zoom x2]">&rarr;</button>
<button onclick="zoomy=zoomy*1.5;placeEvents();" title="[TEXT:zoom x1.5]">&darr;</button>
<button onclick="zoomx=1;zoomy=1;placeEvents();"title="[TEXT:zoom 0]" >0</button>
<button onclick="zoomx=zoomx/2;placeEvents();" title="[TEXT:zoom /2]">&larr;</button>
<button onclick="zoomy=zoomy/1.5;placeEvents();" title="[TEXT:zoom /1.5]">&uarr;</button></td>
<td align="right">
<button onclick="movecal(10)">&gt;[dday10]</button>
<button onclick="movecal(50)">&gt;[dday50]</button>
<button onclick="movecal(100)">&gt;[dday100]</button>[enddate]</td></tr>
</thead>
</table>
<div id="bgmilli"   class="fond">
[BLOCK BAR]
<b id="res[idx]" class="ressource" style="display:none;background-color:[color];">[res]</b>
<div id="bar[idx]" class="event" style="display:none;background-color:[color];"     onclick="subwinevt([FDL_VD2SIZE],[FDL_HD2SIZE],'detailevt','[CORE_STANDURL]app=FDL&action=FDL_CARD&latest=Y&id=[rid]')" onmouseout="unviewdesc(event)" onmouseover="viewdesc(event,[idx])"><img  align="right" src="[evticon]" height="100%"><span style="font-size:7pt">[divtitle]</span></div>
<script>
tevents[[idx]]=['[idx]',[line],[subline],[absx],[absw],'[desc]'];
</script>
[ENDBLOCK BAR]
</div>
<div id="desc" style="display:none" class="description">The description</div>


<div id="cdday"  style="position:absolute;left:0px;top:0px;"></div>
<div id="xgrid"  class="xgrid" ></div>
<div id="ygrid"  class="ygrid" ></div>
