<html>
<head>
<LINK REL="stylesheet" type="text/css" HREF="[CORE_BASEURL]app=CORE&action=CORE_CSS" >
[JS:REF]
<script language="JavaScript">
<!--
[JS:CODE]
//-->
</script>

</head>

<body>

<form name="edit" method="POST">
<input type="hidden" name="id" value="[id]">

<table align="center"  border="0" cellspacing="1" cellpadding="1">
<tr>
<td class="CORETblHeadCell">

  <table align="center"  border="0" cellspacing="0" cellpadding="2">

  <tr>
    <td colspan="4" align="center" class="CORETblHeadCell"><span class="CORETblHead">
    [TITRE] 
    </span></td>
  </tr>
  <tr>
    <td colspan="4" align="left" class="CORETblCell">&nbsp;</td>
  </tr>
  
  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:name] : </span></td>
    <td colspan="3" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" maxlength="30" size="30" name="name" value="[name]"></font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:short_name] : </span></td>
    <td colspan="3" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" maxlength="40" size="30" name="short_name" value="[short_name]"></font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:long_name] : </span></td>
    <td colspan="3" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" maxlength="100" size="30" name="long_name" value="[long_name]"></font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:available] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
    <SELECT NAME="available" SIZE=1>
      [BLOCK SELECTAVAILABLE]

        <option value="[available]" [selected]>[available]</option>

      [ENDBLOCK SELECTAVAILABLE]

    </SELECT
    </span></td>

    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:acl] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" maxlength="20" size="15" name="acl" value="[acl]"></font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:root] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
    <SELECT NAME="root" SIZE=1>
      [BLOCK SELECTROOT]

        <option value="[root]" [selected]>[root]</option>

      [ENDBLOCK SELECTROOT]

    </SELECT
    </span></td>

    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:toc] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
    <SELECT NAME="toc" SIZE=1>
      [BLOCK SELECTTOC]

        <option value="[toc]" [selected]>[toc]</option>

      [ENDBLOCK SELECTTOC]

    </SELECT
    </span></td>
  </tr>

  <tr>
    <td colspan="4" align="left" class="CORETbCell">&nbsp;</td>
  </tr>

  <tr>

    <td colspan="4" align="center" class="CORETblHeadCell">
      <font size="-1"><input class="CORETblHead" type="reset" onClick="self.close()" name="Annuler" value="[TEXT:butclosew]">
      &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <input class="CORETblHead" type="submit" 
        onClick="return Valid_Send()" name="Creer" value="[BUTTONTYPE]">
      </font>
    </td>

  </tr>
  </table>

</td>
</tr>
</table>
</form>
</body>
</head>
