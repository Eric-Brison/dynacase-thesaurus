PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = MAILCONNECTOR
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best
export targetdir PACKAGE

SUBDIR= Class  Images

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) mailconnector.ods \
            RELEASE VERSION 



$(pubdir)/$(appname)/mailconnector.ods : mailconnector.ods
	cp $< $(pubdir)/$(appname)

publish: $(pubdir)/$(appname)/mailconnector.ods 
