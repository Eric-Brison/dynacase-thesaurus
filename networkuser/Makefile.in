PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = NU
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best
TOP_MODULES = ad.php
export targetdir PACKAGE

SUBDIR= Class Api Images

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) networkuser.ods \
            RELEASE VERSION 



$(pubdir)/$(appname)/networkuser.ods : networkuser.ods
	cp networkuser.ods $(pubdir)/$(appname)

publish: $(pubdir)/$(appname)/networkuser.ods