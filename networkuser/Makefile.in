PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = AD
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best
TOP_MODULES = ad.php
export targetdir PACKAGE

SUBDIR= Class Api Images

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) userad.ods \
            RELEASE VERSION 



$(pubdir)/$(appname)/userad.ods : userad.ods
	cp userad.ods $(pubdir)/$(appname)

publish: $(pubdir)/$(appname)/userad.ods