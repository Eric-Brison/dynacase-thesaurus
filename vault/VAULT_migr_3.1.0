#!/bin/bash

if [ "$dbpsql" == "" ]; then
    #load environement variable for freedom
  . /etc/freedom.conf
   wchoose -b
fi

/usr/bin/php -q $wpub/API/updateclass.php --dbcoord="$dbfree" --appc=VAULT --class=VaultDiskStorage

echo "UPDATE vaultdiskstorage set mime_t='', mime_s='', cdate=now(), mdate=now(), adate=now(), teng_state=0, teng_lname='', teng_idfs=-1  where teng_idfs=0;" | psql $dbfree

/usr/bin/php -q $wpub/API/updateclass.php --dbcoord="$dbfree" --appc=VAULT --class=VaultDiskFsStorage
echo "UPDATE vaultdiskfsstorage set fsname='FREEDOM' where fsname is null;" | psql $dbfree
