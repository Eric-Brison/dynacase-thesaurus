[ZONE WGCAL:WGCAL_HTMLHEAD?W=600&H=500]

[IF ShowUsers]<div class="wToolButtonSelect" align="center" style="padding:2pt; margin:4pt" width="100%">
<form id="cUserPref" name="cUserPref" method="POST" action="[CORE_STANDURL]&app=WGCAL&action=WGCAL_PREFS&upref=1">
[TEXT:change user preferences for] : <select id="uid" name="uid" onchange="this.form.submit();">[BLOCK Users]<option value="[uid]" [uselected]>[utext][ENDBLOCK Users]</select>
</form>
</div>[ENDIF ShowUsers]

<script type="text/javascript">
var Zone = new Array();
[BLOCK ZoneJS]Zone[[izone]] = '[tzone]';
[ENDBLOCK ZoneJS]

function SetZone(iz) {
  var e = document.getElementById('p'+iz);
  var b = document.getElementById('b'+iz);
  if (e) {
    for (izx=0; izx<Zone.length; izx++) {
      document.getElementById('p'+Zone[izx]).style.display = 'none';
      document.getElementById('b'+Zone[izx]).className = 'wSmallButtonPulled';
    }
    e.style.display = '';
    b.className = 'wSmallButtonPushed';
  }
}
</script>
<fieldset><legend>[BLOCK ZoneIcons]
<img id="b[tzone]" src="WGCAL/Images/wm-[tzone].gif" height="22" title="[dzone]" class="[IF zSet]wSmallButtonPushed[ENDIF zSet][IFNOT zSet]wSmallButtonPulled[ENDIF zSet]" style="border:0px; cursor:pointer;" onClick="SetZone('[tzone]');" >[ENDBLOCK ZoneIcons]</legend>


[BLOCK ZoneXml]<div id="p[tzone]" style="display:[vzone]">[ZONE WGCAL:WGCAL_PREFS_[azone]?uid=[uid]]</div>[ENDBLOCK ZoneXml]

</fieldset>

