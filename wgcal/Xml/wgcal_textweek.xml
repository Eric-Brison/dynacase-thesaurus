<table  style="padding:1em;" align="center" width="100%" cellspacing="2px">

<tr>
<td align="left" colspan="2" class="wMonthTextTitlePrevNext">
<div style="font-size:120%;" width="100%" align="left">&nbsp;[month] - [TEXT:week] [week]</div>
<div witdh="100%" align="right">[IFNOT mforuser][owner][ENDIF mforuser]
[IF mforuser]<select onchange="document.getElementById('ress').value = this.options[this.selectedIndex].value; document.getElementById('showWeek').submit();">[BLOCK foruser]<option value="[fid]" [selected]>[name][ENDBLOCK foruser]</select>
[ENDIF mforuser]&nbsp;</div></td>
</tr>

<tr class="noprint" valign="top">
<td class="wMonthTextTD" width="[daywidth]px"> [ <span class="wTextNormal" onmouseover="this.className='wTextActive'" onmouseout="this.className='wTextNormal'" onclick="self.print()">[TEXT:print]</span> ] </td>
<td align="right" class="wMonthTextTD" width="[daywidth]px"> [ <span class="wTextNormal" onmouseover="this.className='wTextActive'" onmouseout="this.className='wTextNormal'"  onclick="document.getElementById('week').value = [prevweek]; document.getElementById('showWeek').submit()">[TEXT:week] [lprevweek]</span> | <span class="wTextNormal" onmouseover="this.className='wTextActive'" onmouseout="this.className='wTextNormal'"  onclick="document.getElementById('week').value = [curweek]; document.getElementById('showWeek').submit()">[TEXT:current week]</span> | <span class="wTextNormal" onmouseover="this.className='wTextActive'" onmouseout="this.className='wTextNormal'"  onclick="document.getElementById('week').value = [nextweek]; document.getElementById('showWeek').submit()">[TEXT:week] [lnextweek]</span> ] </td></tr>

<tr valign="top"><td height="[dayheight]px" class="wMonthTextTD" width="[daywidth]px">
<div class="wMonthTextDayTitle"><span class="wMonthTextTitlePrevNext" align="left">&nbsp;[monday_num]&nbsp;</span>&nbsp;[TEXT:monday]</div>
[BLOCK b_monday][eventsdesc][ENDBLOCK b_monday]
</td><td height="[dayheight]px" class="wMonthTextTD" width="[daywidth]px">
<div class="wMonthTextDayTitle"><span class="wMonthTextTitlePrevNext" align="left">&nbsp;[thursday_num]&nbsp;</span>&nbsp;[TEXT:thursday]</div>
