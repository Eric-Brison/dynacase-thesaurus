[IF standalone][ZONE WGCAL:WGCAL_HTMLHEAD][ENDIF standalone]
[IF standalone]<div width="100%" class="WEvTitle">[TEXT:search result for] "[title]"</div>[ENDIF standalone]
[IF noresult]<div width="100%" class="WEvDays">[TEXT:no event]</div>[ENDIF noresult]

[IF styleFIELDSET][BLOCK btime]<fieldset><legend class="WEvDays">[date]</legend>
<table width="100%" cellspacing="0">
[BLOCK devents[date]]<tr valign="top" class="WEvDefault" onmouseover="this.className='WEvOver'" onmouseout="this.className='WEvDefault'" title="[date] [start][IF isHour]-[ENDIF isHour][end], [title]">
<td width="25px"><img width="25px" border="0" src="[icon]"></td>
<td  style="border-bottom:1px solid [bcolor]" nowrap><span>[start][IF isHour] -  [ENDIF isHour][end]</span>
[IF showowner]<br><span style="font-size:80%; font-style:italic">([owner])</span>[ENDIF showowner]
</td>
<td style="border-bottom:1px inset [bcolor];" align="right"><span style="font-weight:bold">[title]</span>[IF HaveDesc]<br><span style="font-size:80%;">[desc]</span>[ENDIF HaveDesc]</td>
</tr>[ENDBLOCK devents[date]]</table>
</fieldset>[ENDBLOCK btime]
[ENDIF styleFIELDSET]

[IF styleTABLE]
<table width="100%" cellspacing="0">
[BLOCK btime]
<tr><td colspan="3" class="WEvDays">[date]</td></tr>[BLOCK devents[date]]
<tr valign="top" class="WEvDefault" onmouseover="this.className='WEvOver'" onmouseout="this.className='WEvDefault'" title="[date] [start][IF isHour]-[ENDIF isHour][end], [title]">
<td width="25px"><img width="25px" border="0" src="[icon]"></td>
<td width="1%"  nowrap><span style="font-size:80%;">[start][IF isHour] -  [ENDIF isHour][end]</span>
[IF showowner]<br><span style="font-size:80%; font-style:italic">([owner])</span>[ENDIF showowner]
</td>
<td ><span style="font-weight:bold">[title]</span>[IF HaveDesc]<br><span style="font-size:80%;">[desc]</span>[ENDIF HaveDesc]</td>
</tr>
[ENDBLOCK devents[date]]
[ENDBLOCK btime]
</table>
[ENDIF styleTABLE]
[IF standalone][ZONE WGCAL:WGCAL_HTMLFOOT][ENDIF standalone]
