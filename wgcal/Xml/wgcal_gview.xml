[IF lmode]<?xml version="1.0" encoding="UTF-8"?>
<freedomsvc uptime="[uptime]" title="Mon agenda"><![CDATA[
[IF noresult]<div>[TEXT:no event]</div>[ENDIF noresult]
[IFNOT noresult]
<table  width="100%" cellspacing="0" >[BLOCK btime]
<tr style="vertical-align:top">
<td colspan="3" style="font-size:90%; font-weight:bold; color:[COLOR_A0]; border-color:[COLOR_A7]; border-width:1px; border-style:none none solid none; padding:5px 0px 0px 0px;" nowrap>[datestr]</td></tr>
[BLOCK devents[date]]
<tr style="vertical-align:top">
<td style="color:[COLOR_A1]; font-size:90%; text-align:right" nowrap>&middot;&nbsp;[hour]&nbsp;:</td>
<td>[Icons]<span style="color:[COLOR_BLACK]">[title]</span>
[IF vLocation] <span style="color:[COLOR_A1]; font-size:90%;">[[location]]</span>[ENDIF vLocation]
[IF showOwner] <span style="color:[COLOR_A1]; font-size:90%; font-style:italic;">([owner])</span>[ENDIF showOwner]
[IF sdescr]<br><span style="color:[COLOR_A1]; font-size:90%">[note]</span>[ENDIF sdescr]
</td></tr>
[ENDBLOCK devents[date]]
[ENDBLOCK btime]</table>[ENDIF noresult]
]]></freedomsvc>[ENDIF lmode]

[IFNOT lmode]

[IF standalone][ZONE WGCAL:WGCAL_HTMLHEAD][ENDIF standalone]
[IF search]<table width="100%" class="wToolButtonUnselect" cellspacing="0" cellpadding="0">
<tr><td>[TEXT:wcal search result for] <span style="background:yellow">[sphrase]</span></td></tr></table>[ENDIF search]


[IF noresult]<div width="100%" class="WEvDays">[TEXT:no event]</div>[ENDIF noresult]
[IFNOT noresult]
<table cellspacing="0" width="100%" >[BLOCK btime]

<tr><td colspan="2" class="WEvDays" style="padding:2px">[datestr]</tr>
[BLOCK devents[date]]<tr valign="top" class="WEvDefault" onmouseover="this.className='WEvOver'" [IF menu]oncontextmenu="this.className='WEvOver'; viewmenu(event,'[menuurl]&ctx=RES&occ=[occ]'); return false"[ENDIF menu] onmouseout="this.className='WEvDefault'">
<td style="border-top: 1px outset white" >[hour]</td><td style="border-top: 1px outset white"  >
<div>[Icons][title] <span style="font-size:80%;">([owner])</span></div>
[IF vNote]<div>[note][ENDIF vNote]</td>
</tr>[ENDBLOCK devents[date]]
[ENDBLOCK btime]</table>[ENDIF noresult]

<div width="100%" class="WEvDays">[ZONE WGCAL:WGCAL_INFOS]</div>
<span id="eventCount" style="display:none">[eventCount]</span>
[IF standalone][ZONE WGCAL:WGCAL_HTMLFOOT][ENDIF standalone]

[ENDIF lmode]


