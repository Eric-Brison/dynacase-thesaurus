[IF styleFIELDSET]
[IF noresult]<div width="100%" class="WEvTitle">[TEXT:no event]</div>[ENDIF noresult]
[BLOCK btime]<fieldset><legend class="WEvTitle">[date]</legend>
<table width="100%" cellspacing="0">
[BLOCK devents[date]]<tr valign="top" class="WEvDefault" onmouseover="this.className='WEvOver'" onmouseout="this.className='WEvDefault'" title="[date] [start][IF isHour]-[ENDIF isHour][end], [title]">
<td  style="border-bottom:1px solid [bcolor]" nowrap><span>[start][IF isHour] -  [ENDIF isHour][end]</span><br><span style="font-size:80%; font-style:italic">([owner])</span></td>
<td style="border-bottom:1px inset [bcolor];" align="right"><span style="font-weight:bold">[title]</span>[IF HaveDesc]<br><span style="font-size:80%;">[desc]</span>[ENDIF HaveDesc]</td>
</tr>[ENDBLOCK devents[date]]</table>
</fieldset>[ENDBLOCK btime]
[ENDIF styleFIELDSET]
[IF styleTABLE]
<table width="100%" cellspacing="0">
[IF noresult]<tr><td colspan="3" class="WEvTitle">[TEXT:no event]</td></tr>[ENDIF noresult]
[BLOCK btime]
<tr><td colspan="2" class="WEvDays">[date]</td></tr>[BLOCK devents[date]]
<tr valign="top" class="WEvDefault" onmouseover="this.className='WEvOver'" onmouseout="this.className='WEvDefault'" title="[date] [start][IF isHour]-[ENDIF isHour][end], [title]">
<td  nowrap><span style="font-size:80%;">[start][IF isHour] -  [ENDIF isHour][end]</span><br><span style="font-size:80%; font-style:italic">([owner])</span></td>
<td ><span style="font-weight:bold">[title]</span>[IF HaveDesc]<br><span style="font-size:80%;">[desc]</span>[ENDIF HaveDesc]</td>
</tr>
[ENDBLOCK devents[date]]
[ENDBLOCK btime]
</table>
[ENDIF styleTABLE]
