[ZONE WGCAL:WGCAL_HTMLHEAD]

<script type="text/javascript">

function SelectRessource(fid, ftitle, ficon, st, tst, cst, romode) {
  var e;
  try {
    top.opener.addRessource(fid, ftitle, ficon, 0, tst, cst, romode);
  } catch(e) {
    top.window.addRessource(fid, ftitle, ficon, 0, tst, cst, 1, romode)
  } ;
  elt = document.getElementById(fid);
  elt.style.display = 'none';
}
</script>


<fieldset><legend>[TEXT:found ressources] ([limit] max)</legend>
[BLOCK RESSOURCES]
<div id="[RESSID]"  class="WGCRessDefault" title="[RESSTITLE]"
     onmouseover="WGCalChangeClass(event, '[RESSID]', 'WGCFamSelected', 'WGCRessOver');" 
     onmouseout="WGCalChangeClass(event, '[RESSID]', 'WGCFamSelected', 'WGCRessDefault');"
     onclick="SelectRessource('[RESSID]', '[RESSTITLEJS]', '[RESSICON]', '[STATE]', '[TSTATE]', '[CSTATE]', [ROMODEV]);" >
  <img height="15"  src="[RESSICON]" title="[RESSTITLE]"/>[RESSTITLE][IF ROMODE]&nbsp;<img height="10px" align="absbottom" title="[TEXT:read only calendar]" src="[IMG:wm-readonly.gif]">[ENDIF ROMODE]
</div>[ENDBLOCK RESSOURCES]
</fieldset> 

[ZONE WGCAL:WGCAL_HTMLFOOT]
