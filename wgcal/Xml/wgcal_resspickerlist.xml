[ZONE WGCAL:WGCAL_HTMLHEAD]

<script type="text/javascript">

function SelectRessource(fid, ftitle, ficon, st, tst, cst) {
  var e;
  try {
    top.opener.addRessource(fid, ftitle, ficon, 0, tst, cst);
  } catch(e) {
    top.window.addRessource(fid, ftitle, ficon, 0, tst, cst, 1)
  } ;
  elt = document.getElementById(fid);
  elt.style.display = 'none';
}

</script>

<fieldset><legend>[TEXT:found ressources]</legend>
[BLOCK RESSOURCES]
<div id="[RESSID]"  class="WGCRessDefault" title="[RESSTITLE]"
     onmouseover="WGCalChangeClass(event, '[RESSID]', 'WGCFamSelected', 'WGCRessOver');" 
     onmouseout="WGCalChangeClass(event, '[RESSID]', 'WGCFamSelected', 'WGCRessDefault');"
     onclick="SelectRessource('[RESSID]', '[RESSTITLE]', '[RESSICON]', '[STATE]', '[TSTATE]', '[CSTATE]');" >
  <img height="15"  src="[RESSICON]" title="[RESSTITLE]"/>[RESSTITLE]
</div>[ENDBLOCK RESSOURCES]
</fieldset>

[ZONE WGCAL:WGCAL_HTMLFOOT]
