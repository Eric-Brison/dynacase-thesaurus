[ZONE WGCAL:WGCAL_HTMLHEAD]
<script type="text/javascript">
var ressId = 0;
var rlist = new Array();
[BLOCK jsRlist]
rlist[[id]] = ['[titlejs]', '[icon]', [romode], [haveDeleg]];[ENDBLOCK jsRlist]

function SelectRessource(id) {
  var e;
  var title = rlist[id][0];
  var icon = rlist[id][1];
  var romode = rlist[id][2];
  var haveDeleg = rlist[id][3];
  try {
    parent.window.addRessource(id, title, icon, 0, '', 0, romode, haveDeleg);
  } catch(e) {
    top.window.addRessource(id, title, icon, 0, '', 0, romode , haveDeleg)
  } ;
  elt = document.getElementById(id);
  elt.style.display = 'none';
}

</script>
<div  style="display:none" id="mRess" class="popup">
<a href="#" id="rcalendar" onclick="subwindow(450, 700, 'iCalendar', '[CORE_STANDURL]&app=WGCAL&action=WGCAL_CALENDAR&sm=1&ress='+ressId); closeMenu('mRess')">[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-refreshcal.gif]" width="15px">&nbsp;[ENDIF POPUPICONS][TEXT:view calendar]</a>
<a href="#" id="rrendezvous" onclick="subwindow(450, 700, 'iNrv', '[CORE_STANDURL]&app=GENERIC&action=GENERIC_EDIT&classid=CALEVENT&wress='+ressId); closeMenu('mRess')">[IF POPUPICONS]<img style="border:0px" src="[IMG:wgcal-small.gif]" width="15px">&nbsp;[ENDIF POPUPICONS][TEXT:m_txt_event_new]</a>
<a href="#" id="radd" onclick="SelectRessource(ressId); closeMenu('mRess')">[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-attendees.gif]" width="15px">&nbsp;[ENDIF POPUPICONS][TEXT:add to viewed calendars]</a>
<a href="#" id="rprefered" onclick="subwindow(450, 700, 'wgcal_hidden', '[CORE_STANDURL]&app=WGCAL&action=WGCAL_ADDTOPREFCALS&id='+ressId); closeMenu('mRess')">[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-newcontact.gif]" width="15px">&nbsp;[ENDIF POPUPICONS][TEXT:add to prefered]</a>
</div>

<div style="border:1px solid  [COLOR_A8]; margin:5px;" onmouseover="if (closeAllMenu) closeAllMenu();" >

<form name="searchical" id="searchical" target="_self" method="POST" action="[CORE_STANDURL]&app=WGCAL&action=WGCAL_SEARCHICAL" style="display:inline">
<div>[TEXT:search cal] : <input id="sical" value="[sical]" name="sical" type="text" style="width:50%" class="textBorder">
</form>
[IF showrlist]
<table width="100%" cellspacing="0" cellpadding="0" onmouseover="if (closeAllMenu) closeAllMenu();" >
<tr><td align="center">([TEXT:limit display to] [max])</td></tr>
[BLOCK rlist]<tr>
<td nowrap id="[id]" class="WGCRessDefault" style="cursor:crosshair" 
    onmouseover="if (this.className=='WGCRessSelected') return; this.className='WGCRessOver'" 
    onmouseout="if (this.className=='WGCRessSelected') return; this.className='WGCRessDefault'"
    onclick="ressId=[id]; subwindow(450, 700, 'iCalendar', '[CORE_STANDURL]&app=WGCAL&action=WGCAL_CALENDAR&sm=1&ress='+ressId);"
    onContextMenu="ressId=[id]; openMenu(event,'mRess', [id]); return false"
    title="[title] : [TEXT:display calendar (m-left-btn) and more... (m-right-btn)]">
&nbsp;<img height="15px" align="absbottom" src="[icon]">&nbsp;[title][IF romode]&nbsp;<img height="10px" align="absbottom" title="[TEXT:read only calendar]" src="[IMG:wm-readonly.gif]">[ENDIF romode]<td>
</tr>[ENDBLOCK rlist]
</table>[ENDIF showrlist]
</div>
[ZONE WGCAL:WGCAL_HTMLFOOT]

