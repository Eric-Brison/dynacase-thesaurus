[ZONE WGCAL:WGCAL_HTMLHEAD]

<div id="wgcalcalendar" width="90%">
   <table width="100%" cellspacing="0" border="0" id="week">
	<tr height="[WGCAL_U_HLINETITLE]px">[F_LINE]</tr>
	<tr height="[WGCAL_U_HLINETITLE]px"><td class="WGCAL_Period" width="[WGCAL_U_HCOLW]px"></td>
	    [BLOCK DAYS_LINE]<td align="center" class="[CSS]">[LABEL]</td>[ENDBLOCK DAYS_LINE]
	</tr>
[BLOCK HOURS]<tr height="[WGCAL_U_HLINEHOURS]px"><td valign="top" align="right" class="[HCLASS]">[HOURR]</td>[C_LINE]</tr>
[ENDBLOCK HOURS]
</table>
</div>

<form target="wgcal_calhidden" style="display:none" id="textform" method="POST">
<input id="Xtext" type="hidden" value="0">
<input id="Ytext" type="hidden" value="0">
<input id="itext" type="text" style="background:lightblue; border:1px inset;" onsubmit="viewNewText(event);" onBlur="viewNewText(event);">
</form>


<script type="text/javascript"> 
window.onresize = WGCalResetSizes;
WGCalViewInit('fevroot', '[IDSTART]', '[IDSTOP]', '[DAYCOUNT]', '[HCOUNT]', '[HSTART]');
</script>

[BLOCK RVS]<script type="text/javascript">WGCalAddEvent('fevroot', [ID], [START], [END])</script>
<div style="display:none">[CONTENT]</div>[ENDBLOCK RVS]


[ZONE WGCAL:WGCAL_HTMLFOOT]
