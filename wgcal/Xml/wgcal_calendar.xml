[ZONE WGCAL:WGCAL_HTMLHEAD]
[IFNOT standAlone][ZONE WGCAL:WGCAL_MENU][ENDIF standAlone]

<div onmouseover="closeAllMenu()" id="root">

<table style="z-index:2000;width:100%;border:0px;" cellspacing="0" id="week">

<tr><td class="WGCAL_Period" colspan="[colspan]">
<div id="slidder" style="width:70%">[ZONE WGCAL:WGCAL_SLIDER]</div>
<div style="text-align:right; width:30%; float:left">[TEXT:week][plusweek] [week] - [month]</div>
</td></tr>

<tr height="[WGCAL_U_HLINETITLE]px">
 <td style="width:[hcolsize]%" class="WGCAL_Period"></td>
[BLOCK DAYS_LINE]<td id="D[IDD]" style="width:[colsize]%" align="center" class="[CSS]" onclick="ClickCalendarCell('[CORE_STANDURL]',2,[times], [timee]);">[LABEL1]</td>[ENDBLOCK DAYS_LINE]
</tr>
[BLOCK HOURS]<tr height="[HLINEHOURS]px">
<td id="L[LID]" style="width:[hcolsize]%" valign="top" align="right" class="[HCLASS]">[HOURR]</td>[C_LINE]
</tr>[ENDBLOCK HOURS]
</table></div>
[ZONE WGCAL:WGCAL_INFOS]

[ZONE WGCAL:WGCAL_POPUP]
[BLOCK EVENTS]
<div id="evt[RG]" class="wEvResume" onclick="subwindow(400, 650, 'ViewEvent[IDP]', '[CORE_STANDURL]&app=[IF edit]GENERIC&action=GENERIC_EDIT[ENDIF edit][IFNOT edit]FDL&action=[IF vRv]IMPCARD[ENDIF vRv][IFNOT vRv]FDL_CARD[ENDIF vRv][ENDIF edit]&id=[IDP]');" [IF hasPCard]onmouseover="closeMenu('calpopup'); WGCalEvOnMouseOver(event, '[RG]');" onmouseout="WGCalEvOnMouseOut(event, '[RG]');" [ENDIF hasPCard]onContextMenu="SetCurrentEvent([IDP], '[TSSTART]'); openMenu(event,'calpopup',[RG]);return false" onMouseDown="if (shiftKeyPushed(event)) { SetCurrentEvent([IDP], '[TSSTART]');openMenu(event,'calpopup',[RG])}">[EvRCard]</div>
[IF hasPCard]<div id="evtc[RG]" class="wEvCard">[EvPCard]</div>[ENDIF hasPCard][ENDBLOCK EVENTS]


<script type="text/javascript"> 
var UrlRoot = '[CORE_STANDURL]';
var CurrentTime = -1;
var Days = new Array();
[BLOCK DAYS]
Days[[iday]] = { start:[days], vstart:[vstart], vend:[vend], view:[view], cpos:0, ev:[] };[ENDBLOCK DAYS]
AltIsFixed = '[ALTFIXED]';
AltTimerValue = '[ALTTIMER]';
WGCalViewInit('[IDSTART]', '[IDSTOP]', [DAYCOUNT], [HCOUNT], [HSTART], [HDIV], [YDURATION]);
[BLOCK EVENTSSC]WGCalAddEvent([RG], [START], [END], '[TITLE]');
[ENDBLOCK EVENTSSC]
WGCalDisplayAllEvents();
</script>


[ZONE WGCAL:WGCAL_HTMLFOOT]
