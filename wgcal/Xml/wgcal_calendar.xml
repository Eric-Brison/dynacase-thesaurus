[ZONE WGCAL:WGCAL_HTMLHEAD]

<div id="root"><table width="100%" cellspacing="0" border="0" id="week">
    <tr height="[WGCAL_U_HLINETITLE]px">[F_LINE]</tr>
    <tr height="[WGCAL_U_HLINETITLE]px">
       <td class="WGCAL_Period" width="[WGCAL_U_HCOLW]px"></td>
	    [BLOCK DAYS_LINE]<td id="D[IDD]" align="center" class="[CSS]">[LABEL]</td>[ENDBLOCK DAYS_LINE]
	</tr>
	[BLOCK HOURS]
        <tr height="[HLINEHOURS]px">
           <td id="L[LID]" width="[WGCAL_U_HCOLW]px" valign="top" align="right" class="[HCLASS]">[HOURR]</td>[C_LINE]</tr>
[ENDBLOCK HOURS]
</table></div>

<form target="wgcal_calhidden" style="display:none" id="textform" method="POST">
<input id="Xtext" type="hidden" value="0">
<input id="Ytext" type="hidden" value="0">
<input id="itext" type="text" style="background:lightblue; border:1px inset;" onsubmit="viewNewText(event);" onBlur="viewNewText(event);">
</form>


<script type="text/javascript"> 
window.onresize = WGCalResetSizes;
[BLOCK DAYS]Days[[iday]] = [days];
[ENDBLOCK DAYS]
WGCalViewInit('[IDSTART]', '[IDSTOP]', '[DAYCOUNT]', '[HCOUNT]', '[HSTART]');
</script>

[BLOCK EVENTS]
<div id="evt[ID]" 
     onclick="subwindow(300, 500, 'EditEvent[ID]', '[CORE_STANDURL]&app=WGCAL&action=WGCAL_EDITEVENT&evt=-1');"  
     onmouseover="WGCalEvOnMouseOver(event, '[ID]');" 
     onmouseout="WGCalEvOnMouseOut(event, '[ID]');" 
    style="cursor:pointer; background-color:red; border:0px; display:none">
<div id="evtt[ID]" style="border:1px outset; background-color:grey; height:4px"></div>
<div id="evta[ID]" style="border:1px outset; background-color:white;">[ABSTRACT]</div>
</div>
<div id="evtc[ID]" style="background-color:black; color:white; border:2px outset solid; display:none">[LONGDESCR]</div> 
[ENDBLOCK EVENTS]
<script type="text/javascript">
[BLOCK EVENTSSC]WGCalAddEvent('[IDSTART]', [ID], [START], [END], [SHIFT]);
[ENDBLOCK EVENTSSC]
</script>


[ZONE WGCAL:WGCAL_HTMLFOOT]
