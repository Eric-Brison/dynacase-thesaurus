[ZONE WGCAL:WGCAL_HTMLHEAD]
<script type="text/javascript">

  var myframe = parent.document.getElementById('wgcal_conflict');
  [IF NOCF]ignoreConflict();[ENDIF NOCF]
  [IFNOT NOCF] myframe.style.display = '';[ENDIF NOCF]


  var IdOpened = -1;

  function ShowElt(t, id) {
    var evtc = document.getElementById('evtc'+id);
    evtc.style.display = '';
    evtc.style.position = 'absolute';
    evtc.style.width = 'auto';
    evtc.style.zIndex = 1001;
    evtc.style.display = '';
    ww = getFrameWidth();
    wh = getFrameHeight();
    var h = getObjectHeight(evtc);
    var w = getObjectWidth(evtc);
    evtc.style.left = (ww-w-10)+'px';
    evtc.style.top = (wh-h-10)+'px';
    IdOpened = id;
    t.className = 'WGCFamSelected';
 }

  function HideElt(t) {
    if (IdOpened!=-1) document.getElementById('evtc'+IdOpened).style.display = 'none';
    IdOpened = -1;
    t.className = 'WGCRessDefault';
  }

  function ignoreConflict() {
    var refi = parent.document.getElementById('editevent');
    refi.action = '[CORE_STANDURL]&app=WGCAL&action=WGCAL_STOREEVENT';
    refi.target = 'wgcal_calendar';
    refi.cfchecked.value = 1;
    refi.submit();
    myframe.style.display = 'none';
    parent.window.close();  
  }

  function processConflict() {
    myframe.style.display = 'none';
  }

</script>

<div style="padding:2em;">

[IFNOT NOCF]
<table  style="border:thin inset;" align="center" width="100%" cellspacing="0" cellpadding="2px"><tr>
<tr><td class="wMonthTextTitle">[TEXT:there are conflicts with events (see after)].</td></tr>
[BLOCK CONFLICTS]
<tr><td class="WGCRessDefault" style="width:100%" onmouseover="HideElt(this); ShowElt(this,[ID]);" onmouseout="HideElt(this);">
[EvRCard]</td></tr>[ENDBLOCK CONFLICTS]
<tr><td width="100%" class="wMonthTextTitle" align="right">
<img src="[IMG:wm-cancel.gif]" height="15" title="[TEXT:modify event]" class="WGCalAction" onclick="processConflict();">&nbsp;&nbsp;
<img src="[IMG:wm-accept.gif]" height="15" title="[TEXT:ignore conflict]" class="WGCalAction" onclick="ignoreConflict();">&nbsp;&nbsp;
</td>
</tr></table>[ENDIF NOCF]

[BLOCK CARDS]
<div id="evtc[ID]" style="display:none; border:1px outset; background-color:white');">[EvPCard]</div>[ENDBLOCK CARDS]

</div>
[ZONE WGCAL:WGCAL_HTMLFOOT]
