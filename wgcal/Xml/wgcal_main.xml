<frameset name="wgcal" id="wgcal" frameborder="0" framespacing="0" border="0" cols="[WGCAL_U_LEFTWIDTH]pt,*">

  <frame name="wgcal_toolbar" src="[CORE_STANDURL]&app=WGCAL&action=WGCAL_TOOLBAR" scrolling="needed">

  <frameset name="wgcal_frmain" id="wgcal_frmain" frameborder="0" framespacing="0" border="0" rows="25px,*,0pt">
    <frame name="wgcal_menubar"  src="[CORE_STANDURL]&app=WGCAL&action=WGCAL_MENUBAR" scrolling="no">
    <frame name="wgcal_calendar" src="[CORE_STANDURL]&app=WGCAL&action=WGCAL_CALENDAR" scrolling="none">
    <frame name="wgcal_hidden"   src="[CORE_STANDURL]&app=WGCAL&action=WGCAL_HIDDEN" noresize style="border-style:none" scrolling="needed">
  </frameset>

</html>
