<!-- Start resume [REF]::[ID] -->
<div id="evtc[REF]" style="display:none; border:1px outset; background-color:[bgcolor]">
<div style="background-color:[bgcolor]; border : 4px solid white; height:100%;">
<table cellpadding="2px" cellspacing="0" border="0">
<tr>
  <td width="15" style="border: 1px outset; background-color:[bgstate]" valign="top" rowspan="3">[BLOCK icons]<img style="pointer:crosshair; border:0px" src="[iconsrc]" width="15" title="[icontitle]"><br>[ENDBLOCK icons]</td>
  <td valign="top" colspan="2" style="background-color:[bgheadcolor]; color:white">
    <table width="100%" cellpadding="0" cellspacing="0" border="0"><tr>
    <td align="left" style="background-color:[bgheadcolor]; color:white"><span style="font-size:120%; font-weight:bold">[TITLE]</span></td>
    <td align="right"><img src="[iconevent]" height="20"></td>
    </tr></table>
 </td>
</tr>
<tr>
  <td valign="top">
<table cellpadding="0px" cellspacing="0" border="0">
<tr><td style="font-weight:bold">[DSTART]&nbsp;</td><td style="font-weight:bold">[START]</td></tr>
<tr><td style="font-weight:bold">[DEND]</td>&nbsp;<td style="font-weight:bold">[END]</td></tr>
</table>
<br><br>
<span style="display:[repeatdisplay]">[TEXT:repeat] [repeatinfos]</span><br>
<span style="font-style:italic; display:[repeatdisplay]">[repeatuntil]</span><br><br>
[TEXT:owner] : <span style="font-style:italic">[owner]</span><br>
[TEXT:modified] : <span style="font-style:italic">[modifdate]</span><br>
</td>
  <td valign="top">
<fieldset style="display:[attdisplay]"><legend>[TEXT:attendees]</legend>
[BLOCK attlist]<div><img height="15" src="[atticon]">[atttitle]&nbsp;<span style="font-style:italic">([attstate])</span></div>
[ENDBLOCK attlist]
</fieldset></td>
</tr>
<tr style="display:[displaynote]">
  <td colspan="2" valign="top">
<fieldset><legend>[TEXT:note]</legend>
[NOTE]</fieldset></td>
</tr>
</table>
</div>
