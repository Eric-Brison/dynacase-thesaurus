 [BLOCK ISCONF]onclick="subwindow(400, 650, 'EditEvent[REF]', '[CORE_STANDURL]&app=WGCAL&action=WGCAL_EDITEVENT&evt=[ID]');"[ENDBLOCK ISCONF]
