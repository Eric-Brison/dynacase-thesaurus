
<!-- Popup Options -->
<div  style="z-index:3000; display:none" id="wgcal_m_options" class="wgcalpopup">
[BLOCK OPTIONS_MENU]
<a href="#" id="[item_id]" onclick="activate(this,'[item_onclick]'); closeMenu('wgcal_m_options')">[IF POPUPICONS]<img style="border:0px" src="WGCAL/Images/[item_icon]" height="12px">&nbsp;[ENDIF POPUPICONS][item_label]</a>
[ENDBLOCK OPTIONS_MENU]
<a class="separator"></a>
<a href="#" id="close_wgcal_m_options" onclick="closeMenu('wgcal_m_options')">[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-closepopup.png]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:close menu]</a>
</div>

<table cellspacing="0" border="0" width="100%" style="position:fixed;  z-index:500;">
<tr>
<!-- 
<td align="left" width="50" class="wMainMenu">&nbsp;File</td>
-->
<td align="left" width="50" class="wMainMenu"
  onClick="openMenu(event,'wgcal_m_options', 1); return false" >&nbsp;Options&nbsp;</td>
<td class="wMainMenu"></td>
<!-- 
<td align="center" class="wMainMenu" width="50" >
<img height="16" style="cursor:pointer" title="[TEXT:add a calendar]" onclick="subwindow(60, 400, 'Add a Calendar', '[CORE_STANDURL]&app=WGCAL&action=WGCAL_CREATECALENDAR');" src="WGCAL/Images/wgcal-addcalendar.png" 
></td>
-->
<td align="center" class="wMainMenu" width="80">
<img height="16" style="cursor:pointer" title="[TEXT:week view]"  onclick="setDaysViewed(7);" src="WGCAL/Images/oneweek.png">
<img height="16" style="cursor:pointer" title="[TEXT:two week view]"  onclick="setDaysViewed(14);" src="WGCAL/Images/twoweek.png">
<img height="16" style="cursor:pointer" title="[TEXT:month text view]" onclick="setTextView(-1);" src="WGCAL/Images/wm-viewtext.png">
</td>
<td align="right" width="50" class="wMainMenu">
<img height="16" title="[TEXT:print view]"  style="cursor:pointer" onclick="parent.wgcal_calendar.print()" src="WGCAL/Images/wm-print.png">
<img id="menu_prefs" title="[TEXT:change preferences]" style="cursor:pointer" onclick="subwindow(300, 500, 'EditPreferences', '[CORE_STANDURL]&app=WGCAL&action=WGCAL_PREFS');" src="WGCAL/Images/wgcal-prefs.png" height="16" >
</td>
</tr>
</table>
