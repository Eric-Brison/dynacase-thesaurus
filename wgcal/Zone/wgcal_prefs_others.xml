<h4>[TEXT:others]</h4>
<table width="100%" cellspacing="0" style="margin:0px;">

<tr class="wTextActive"><td colspan="3">[TEXT:options]</td></tr>

[BLOCK MAILOPTCHK]<tr><td></td><td align="left" colspan="2"><span style="cursor:pointer;" onmouseover="this.className='wTextActive'" onmouseout="this.className='wTextNormal'" onclick="SetChkPref('[idoption]', '[paramoption]', '[trefresh]', '[arefresh]');" title="[textoption]"><input id="[idoption]" name="[idoption]" type="checkbox" style="cursor:pointer;" onclick="SetChkPref('[idoption]', '[paramoption]', '[trefresh]', '[arefresh]');" [stateoption]> [textoption]</span></td></tr>[ENDBLOCK MAILOPTCHK]


<tr class="wTextActive"><td colspan="3">[TEXT:toolbar tools]</td></tr>

[BLOCK TOOLBARTOOLS]<tr><td></td><td align="left" colspan="2"><span style="cursor:pointer;" onmouseover="this.className='wTextActive'" onmouseout="this.className='wTextNormal'" onclick="SetChkPref('[idoption]', '[paramoption]', '[trefresh]', '[arefresh]');" title="[textoption]"><input id="[idoption]" name="[idoption]" type="checkbox" style="cursor:pointer;" onclick="SetChkPref('[idoption]', '[paramoption]', '[trefresh]', '[arefresh]');" [stateoption]> [textoption]</span></td></tr>[ENDBLOCK TOOLBARTOOLS]

<tr class="wTextActive"><td colspan="3">[TEXT:portal]</td></tr>

[BLOCK PORTAL]<tr>
<td align="right" colspan="2">[iText] </td>
<td>
<select style="cursor:pointer" id="[iSel]" onchange="mytoto('[iVar]', this.options[this.selectedIndex].value, 'wgcal_toolbar', 'WGCAL_TOOLBAR');">
[BLOCK OPT[iSel]]<option value="[iOptVal]" [iOptSel]>[iOptText][ENDBLOCK OPT[iSel]]
</select>
</td>
</tr>[ENDBLOCK PORTAL]

</table>


