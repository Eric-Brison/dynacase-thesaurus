<tr><td width="10%"></td><td align="right">[TEXT:theme caracteristics] : </td><td><select style="cursor:pointer" id="theme" onchange="mytoto('WGCAL_U_THEME', this.options[this.selectedIndex].value, 'wgcal_calendar', 'WGCAL_CALENDAR');idf=subwindow('','','wgcal_toolbar'); idf.top.location.reload(true); document.location.reload(true);">[BLOCK THEME]<option value="[optvalue]" [optselect]>[optdescr][ENDBLOCK THEME]</select></td></tr>

<tr><td width="10%"></td><td align="right">[TEXT:waiting rv window size] : </td><td><select style="cursor:pointer" id="theme" onchange="mytoto('WGCAL_U_ZWRVALERTSIZE', this.options[this.selectedIndex].value, 'wgcal_toolbar', 'WGCAL_TOOLBAR');">[BLOCK ZRVWS]<option value="[optvalue]" [optselect]>[optdescr][ENDBLOCK ZRVWS]</select></td></tr>


