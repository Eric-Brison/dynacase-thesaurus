<tr><td width="10%"></td><td align="right">[TEXT:display popup position] : </td><td><select style="cursor:pointer" id="altposition" onchange="mytoto('WGCAL_U_ALTFIXED', this.options[this.selectedIndex].value, 'wgcal_calendar', 'WGCAL_CALENDAR')">[BLOCK ALTPOS]<option value="[optvalue]" [optselect]>[optdescr][ENDBLOCK ALTPOS]</select></td></tr>

<tr><td width="10%"></td><td align="right">[TEXT:timeout before displaying popup] : </td><td><select style="cursor:pointer" id="alttimeout" onchange="mytoto('WGCAL_U_ALTTIMER', this.options[this.selectedIndex].value, 'wgcal_calendar', 'WGCAL_CALENDAR')">[BLOCK ALTTIMER]<option value="[optvalue]" [optselect]>[optdescr][ENDBLOCK ALTTIMER]</select></td>
</tr>

