[BLOCK OPTCHK]<tr><td width="10%"></td><td align="right"><span style="cursor:pointer;" onmouseover="this.className='wTextActive'" onmouseout="this.className='wTextNormal'" onclick="SetChkPref('[idoption]', '[paramoption]', '[trefresh]', '[arefresh]');" title="[textoption]">[textoption] : <input id="[idoption]" name="[idoption]" type="checkbox" style="cursor:pointer;" onclick="SetChkPref('[idoption]', '[paramoption]', '[trefresh]', '[arefresh]');" [stateoption]></span></td><td></td></tr>[ENDBLOCK OPTCHK]

