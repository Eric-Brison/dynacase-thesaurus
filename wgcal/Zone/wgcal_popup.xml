<!-- POPUP DIVISION -->
<div  style="z-index:2000; display:none" id="calpopup" class="wgcalpopup">
[BLOCK POPUPITEMS]
<a href="#" id="[item]" onclick="activate(this,'[CORE_STANDURL]&app=[app]&action=[action]&ev='+EvSelected+'[params]','');
closeMenu('calpopup')">[label]</a>[ENDBLOCK POPUPITEMS]

<!-- a class="separator"></a -->


<a href="#" id="acceptrv" onclick="activate(this,'[CORE_STANDURL]&app=WGCAL&action=WGCAL_SETEVENTSTATE&id='+EvSelected+'&st=2','');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-evaccept.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:accept this]</a>
<a href="#" id="rejectrv" onclick="activate(this,'[CORE_STANDURL]&app=WGCAL&action=WGCAL_SETEVENTSTATE&id='+EvSelected+'&st=3','');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-evrefuse.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:reject this]</a>
<a href="#" id="tbcrv" onclick="activate(this,'[CORE_STANDURL]&app=WGCAL&action=WGCAL_SETEVENTSTATE&id='+EvSelected+'&st=4','');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-evconfirm.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:to be confirm this]</a>

<a class="separator"></a>

<a href="#" id="editrv" onclick="activate(this,'[CORE_STANDURL]&app=GENERIC&action=GENERIC_EDIT&id='+EvSelected,'wgcal_edit');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-evedit.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:edit this]</a>
<a href="#" id="deloccur" onclick="activate(this,'[CORE_STANDURL]&app=WGCAL&action=WGCAL_DELOCCUR&id='+EvSelected+'&evocc='+EvCurDay,'wgcal_calendar');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-deloccur.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:delete this occurence]</a>
<a href="#" id="viewrv" onclick="activate(this,'[CORE_STANDURL]&app=FDL&action=IMPCARD&id='+EvSelected,'wgcal_view');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-evview.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:view this]</a>
<a href="#" id="deleterv" onclick="ok =confirm('[TEXT:confirm delete for this event] ?'); if (ok) activate(this,'[CORE_STANDURL]&app=WGCAL&action=WGCAL_DELETEEVENT&id='+EvSelected,'');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-evdelete.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:delete this]</a>

<a href="#" id="historyrv" onclick="activate(this,'[CORE_STANDURL]&app=WGCAL&action=WGCAL_HISTO&id='+EvSelected,'wgcal_history');closeMenu('calpopup')" >
[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-evhistory.gif]" height="12px">&nbsp;[ENDIF POPUPICONS][TEXT:history]</a>

<a class="separator"></a>
<a href="#" id="cancelrv" onclick="closeMenu('calpopup')"  >[IF POPUPICONS]<img style="border:0px" src="[IMG:wm-closepopup.gif]" height="15px">&nbsp;[ENDIF POPUPICONS][TEXT:close menu]</a>

</div> <!-- end POPUP DIVISION -->



