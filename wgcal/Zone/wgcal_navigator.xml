<form id="dispNav" target="wgcal_hidden" name="dispNav" method="POST"  
      action="[CORE_STANDURL]&app=WGCAL&action=WGCAL_SETTOOLSTATE"><input name="tool" type="hidden" value="0">
</form>

<fieldset id='vinav' class="WGCListRess">
<legend class="WGCToolTitle" style="cursor:pointer"
    onClick="WGCalImgAltern(event, 'idimgnav', 'WGCAL/Images/wgcal-arrowup.gif', 'WGCAL/Images/wgcal-arrowdown.gif');
             WGCalChangeVisibility(datePickerVisible, 'inav'); ">
    <img id="idimgnav" src="WGCAL/Images/wgcal-arrow[VISICO].gif" title="[TEXT:calendar selector]">
     [TEXT:date selector]</legend>

<div id="inav" style="display:none">
<script type="text/javascript">
    Calendar.setup({
        ifFormat       :    '%s',       // format of the input field
        showsTime      :    false,            // will display a time selector
        singleClick    :    false,           // double-click mode
        firstDay       :    1,
        date           :    new Date([CmTIME]),
        weekNumbers    :    true,
        flat           :    'inav',
        range          :    [ [YSTART] , [YSTOP] ],
        flatCallback   :    WGCalSetDate,
        step           :    1              
    });
</script>
</div>
</fieldset>

<form id="fdatesel" target="wgcal_calendar" name="datesel" method="POST"  action="[CORE_STANDURL]&app=WGCAL&action=WGCAL_CALENDAR">
  <input type="hidden" id="iodate" value="[CTIME]" name="olddate">
  <input type="hidden" id="indate" value="-2" name="newdate">
</form>
