<div  width="100%" class="wMonthTextDayTitle"  onclick="ClickCalendarCell('[CORE_STANDURL]',0,[timeb], [timee]);">
<span width="30%" class="wMonthTextTitlePrevNext" align="left">&nbsp;[daynum]&nbsp;</span>
<span width="70%" style="font-size:80%; text-align:right">[daylabel]</span>
</div>
[BLOCK HLine]<div class="WEvDefault"
  onclick="subwindow(400, 650, 'ViewEvent[IDP]', '[CORE_STANDURL]&app=[IF edit]GENERIC&action=GENERIC_EDIT[ENDIF edit][IFNOT edit]FDL&action=IMPCARD[ENDIF edit]&id=[IDP]');"
onmouseover="this.className='WEvOver'; WGCalEvOnMouseOver(event, '[RG]');" 
onmouseout="this.className='WEvDefault';WGCalEvOnMouseOut(event, '[RG]');" 
onContextMenu="SetCurrentEvent([IDP], '[TSSTART]'); openMenu(event,'calpopup',[RG]);return false" 
onMouseDown="if (shiftKeyPushed(event)) { SetCurrentEvent([IDP], '[TSSTART]');openMenu(event,'calpopup',[RG])}"
style="cursor:crosshair" >[EvSTCard]</div>[ENDBLOCK HLine]
