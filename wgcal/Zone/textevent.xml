<div  width="100%" class="wMonthTextDayTitle"  onclick="subwindow(400, 650, 'EditEventNew', '[CORE_STANDURL]&app=GENERIC&action=GENERIC_EDIT&classid=CALEVENT&nh=0&ts=[timeb]&te=[timee]')">
<span width="30%" class="wMonthTextTitlePrevNext" align="left">&nbsp;[daynum]&nbsp;</span>
<span width="70%" style="font-size:80%; text-align:right">[daylabel]</span>
</div>

[BLOCK HLine]<div style="overflow:hidden; cursor:crosshair" oncontextmenu="fcalCancelEvDisplay(event, [rg]); fcalOpenMenuEvent(event, [rg], this)" onmouseover="fcalStartEvDisplay(event, [rg]);" onmouseout="fcalCancelEvDisplay(event, [rg]);" >
<div class="WEvDefault" style="border-right: 5px solid [ownercolor]; font-weight:bolder; font-size:80%;">[hours]&nbsp;[icolist][IF showOwner]&nbsp;<span style="font-weight:normal;font-style:italic;">([owner])</span>[ENDIF showOwner]</div>
<div style="padding-left:5px; padding-bottom:5px">
<div style="color:black">[evt_title]</div>
[IF otherInfos]<div style="font-size:80%; font-style:italic">
[IF haveLocation][TEXT:location_1stUpcase] : [location][ENDIF haveLocation]
[IF haveCat]&nbsp; [TEXT:categorie] : [categorie][ENDIF haveCat]
</div>[ENDIF otherInfos]</div>
</div>[ENDBLOCK HLine]
