PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir = @PUBRULE@
appname = @PACKAGE@
pubdir  = @prefix@
srcdir  = @srcdir@


export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR=  Class Images Zone Xml Action Css Javascript

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) \
	$(PACKAGE).app \
        RELEASE VERSION

$(pubdir)/API:
	mkdir -p $@

clean:
	rm -f config.log config.status configure WGCAL-*.tar.gz WGCAL_init.php WGCAL.spec 
	rm -rf autom4te-2.53.cache
	find . -name \*~  -exec rm {} \;

