PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = DAV
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR=  Class Actions Javascript Images Tools

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) DAV_conf.apache.in \
            RELEASE VERSION   

$(pubdir)/$(appname)/DAV_conf.apache: DAV_conf.apache
	cp $< $@

publish:$(pubdir)/$(appname)/DAV_conf.apache