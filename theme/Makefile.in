PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = @APPNAME@
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR= Style debian
TOP_MODULES =  wtheme

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) wtheme \
            RELEASE VERSION 

$(pubdir):
	mkdir -p $(pubdir)
$(pubdir)/wtheme: wtheme $(pubdir)
	cp $< $@

