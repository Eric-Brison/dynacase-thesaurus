PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir = @PUBRULE@
# appname = @PACKAGE@
appname = freedomWS
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR= Class Wsdl Action Tools

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) \
            RELEASE VERSION  	    


$(pubdir)/$(appname)/apache_ws.conf: apache_ws.conf $(pubdir)/$(appname)
	cp $< $(pubdir)/$(appname)


publish: $(pubdir)/$(appname)/apache_ws.conf $(pubdir)/$(appname)
