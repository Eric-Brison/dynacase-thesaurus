PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = @PACKAGE@
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR= Class

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) \
            RELEASE VERSION  	    


$(pubdir)/$(appname)/apache_ws.conf: apache_ws.conf
	cp $< $(pubdir)/$(appname)


publish: $(pubdir)/$(appname)/apache_ws.conf
