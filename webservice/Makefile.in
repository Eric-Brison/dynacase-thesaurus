PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir = @PUBRULE@
# appname = @PACKAGE@
appname = 
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR= Class Wsdl Action

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) \
            RELEASE VERSION  	    


$(pubdir):
	mkdir -p $(pubdir)

$(pubdir)/apache_ws.conf: apache_ws.conf $(pubdir)
	cp $< $(pubdir)


publish: $(pubdir)/apache_ws.conf
